          <Css><![CDATA[#this >.field:not(:last-child) {
  margin-bottom: 3px;
}
>.field:first-child {
  width: 180px;
  margin-left: 110px;
  border: 1px solid #F9F5F1;
}
>.field:last-child {
  width: 180px;
  margin-left: 110px;
  border: 1px solid #F9F5F1;
}
]]></Css>
        </Layout>
        <Field t="css_dt.BorderSequence" Prefix="top"/>
