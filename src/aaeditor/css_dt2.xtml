                <UIHints t="number.FormatUIHints" InitialPixelsPerUnit="1" Step="1"/>
              </Format>
            </FieldType>
            <FieldAspect t="field_feature.Layout">
              <Css><![CDATA[#wrapper { margin-top: 30px; margin-left: 1px; }
]]></Css>
            </FieldAspect>
          </Field>
        </Field>
        <Field t="field.ItemListContainer" ID="breaks_cntr" Title="color breaks">
          <Items t="itemlist.XmlItems" ParentXml="%%" Tag="break"/>
          <Field t="field.Layout" ID="breaks_cntr_Toolbar" Title="Toolbar">
            <Layout t="layout.HorizontalHtmlTable" Spacing="5px"/>
