            <Style t="layout.HorizontalCss3" Spacing="21px"/>
            <Css><![CDATA[#this {
  display: -webkit-box;
  -webkit-box-orient: horizontal;
}
#this >.field {
  margin-right:23px;
  width: 180px;
  border: 1px solid #F9F5F1;
}]]></Css>
          </Layout>
          <Field t="css_dt.BorderSequence" Prefix="right"/>
        </Field>
        <Field t="css_dt.BorderSequence" Prefix="bottom"/>
      </Field>
      <FieldAspect t="field_feature.Layout">
        <Css><![CDATA[#this { margin-top: 24px; margin-left: 0; }
#wrapper { }
]]></Css>
      </FieldAspect>
    </Field>
    <FieldAspect t="field_aspect.Variable" VarName="BorderType">
      <Value value="%!border_width/@border_type%"/>
    </FieldAspect>
  </xtml>
</Component>

