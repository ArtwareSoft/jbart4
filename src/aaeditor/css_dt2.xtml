<xtml ns="css_dt" light="false">

    <Component id="PositivePx" type="number.Format">
      <xtml t="number.Format" Min="0" Max="" Symbol="px" DataString="%Value%%Symbol%" UseDynamicSymbol="true">
        <DisplayString t="number.ByExpression" Expression="%Value%%Symbol%"/>
      </xtml>
    </Component>

    <Component id="OffsetPx" type="number.Format">
      <xtml t="number.Format" Min="" Max="" Symbol="px" DataString="%Value%%Symbol%" UseDynamicSymbol="true">
        <DisplayString t="number.ByExpression" Expression="%Value%%Symbol%"/>
      </xtml>
    </Component>

    <Component id="ColorLuminance" type="data.Data" execution="native">
      <Param name="Color" essential="true"/>
      <Param name="Luminance" description="0-1" essential="true"/>
    </Component>

    <Component id="LuminanceOfColors" type="data.Data" execution="native">
      <Param name="BaseColor" essential="true"/>
      <Param name="Color1" essential="true"/>
      <Param name="Color2" essential="true"/>
    </Component>

    <Component id="RecalcGradient" type="action.Action">
      <Param name="BackgroundImageElem"/>    
      <xtml t="xml.ChangeXml" Xml="%%" Data="%$BackgroundImageElem%">
        <Var name="BaseColor" value="%../background_color/@value%"/>    
        <Var name="Color1" t="css_dt.ColorLuminance" Luminance="%@luminance%" Color="%$BaseColor%"/>    
        <Var name="Color2" t="css_dt.ColorLuminance" Luminance="-%@luminance%" Color="%$BaseColor%"/>    
        <Change t="xml.DeleteChildren"/>    
        <Change t="xml.AddChildren" CloneChildren="true">
          <Children t="xml.Xml" DynamicContent="true">
            <ColorStop color="%$Color1%" pos="0\%"/>    
          </Children>
        </Change>
        <Change t="xml.AddChildren" CloneChildren="true">
          <Children t="xml.Xml" DynamicContent="true">
            <ColorStop color="%$BaseColor%" pos="50\%"/>    
          </Children>
        </Change>
        <Change t="xml.AddChildren" CloneChildren="true">
          <Children t="xml.Xml" DynamicContent="true">
            <ColorStop color="%$Color2%" pos="100\%"/>    
          </Children>
        </Change>
      </xtml>
    </Component>

<Component id="CssEditor" type="section.Style">
  <xtml t="ui.CustomStyle" base="section.Grey">
    <Html><![CDATA[<div>
  <div class="section_title_top">
    <div class="section_title"/>
  </div>
  <div class="section_body"/>
</div>]]></Html>
    <Css><![CDATA[#this { width: 400px; }
  #this >.section_title_top>.section_title {
  padding-bottom: 3px;
  color: #666;
  font-weight: bold;
  font-size: 11px;
  font-family: "lucida grande",tahoma,verdana,arial,sans-serif;
  text-transform:capitalize;
}

#this >.section_title_top {
}
]]></Css>
    <Javascript><![CDATA[function(section) {  
  section.setInnerHTML('.section_title',section.Title);  
  section.addSectionBody('.section_body');
}]]></Javascript>
  </xtml>
</Component>

<Component id="MarginPadding" type="field.Fields">
  <Param name="Type" type="enum" Options="padding,margin"/>
  <xtml t="control.Layout" ID="%$Type%" Title="%$Type%">
    <FieldAspect t="field_aspect.FieldData" FieldData="%{$Type}%"/>
    <Style t="properties.TitlesAboveFields" HorizontalSpacing="15px" VerticalSpacing="8px"/>
    <Field t="control.Layout" ID="Options" Title="Options">
      <Layout t="layout.Vertical" Spacing=""/>
      <Field t="fld.Field" FieldData="%!@type%" ID="type" Title="Type" gradientType="linear">
        <FieldType t="fld_type.Picklist" AllowEmptyValue="false" AllowValueNotInOptions="">
          <Options t="editable_picklist.OptionsByCommas" Options="separated,xy,unified"/>
          <Style t="editable_picklist.RadioFadeRight">&#xa;              </Style>
        </FieldType>
        <FieldAspect t="field_feature.HandleEvent">
          <Event t="field_feature.Update"/>
          <Action t="operation.RunActions">
            <Action t="field.RefreshField" FieldID="Options" Scope="screen" FireOnUpdate=""/>
          </Action>
        </FieldAspect>
        <FieldAspect t="field_aspect.DefaultValue" Value="separated"/>
      </Field>
      <Field t="control.Layout" ID="Separated" Title="Separated">
        <Layout t="layout.Vertical" Spacing="0"/>
        <Field t="fld.Field" FieldData="%!@top%" ID="Top" Title="Top">
          <FieldType t="fld_type.EditableNumber" NumberValidation="true">
            <Style t="editable_number.Slider" Width="150px"/>
            <Format t="css_dt.OffsetPx"/>
          </FieldType>
          <FieldAspect t="field_feature.Layout">
            <Css><![CDATA[#this { margin-left: 96px; }
#wrapper { }
]]></Css>
          </FieldAspect>
        </Field>
        <Field t="control.Layout" ID="Horizontal" Title="Horizontal">
          <Layout t="layout.HorizontalCss3" Spacing="20px"/>
          <Field t="fld.Field" ID="left" Title="Left" FieldData="%!@left%">
            <FieldType t="fld_type.EditableNumber" NumberValidation="true">
              <Style t="editable_number.Slider" Width="150px"/>
              <Format t="css_dt.OffsetPx"/>
            </FieldType>
          </Field>
          <Field t="fld.Field" FieldData="%!@right%" ID="Right" Title="Right">
            <FieldType t="fld_type.EditableNumber" NumberValidation="true">
              <Style t="editable_number.Slider" Width="150px"/>
              <Format t="css_dt.OffsetPx"/>
            </FieldType>
          </Field>
        </Field>
        <Field t="fld.Field" ID="bottom" Title="Bottom" FieldData="%!@bottom%">
          <FieldType t="fld_type.EditableNumber" NumberValidation="true">
            <Style t="editable_number.Slider" Width="150px"/>
            <Format t="css_dt.OffsetPx"/>
          </FieldType>
          <FieldAspect t="field_feature.Layout">
            <Css><![CDATA[#this { margin-left: 96px; }
#wrapper { }
]]></Css>
          </FieldAspect>
        </Field>
        <FieldAspect t="field_aspect.Hidden">
          <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render">
            <ShowCondition t="yesno.OR" Data="%@type%">
              <Item t="yesno.EqualsTo" To="separated"/>
              <Item t="yesno.IsEmpty"/>
            </ShowCondition>
          </OnCondition>
        </FieldAspect>
      </Field>
      <Field t="control.Layout" ID="XY" Title="XY">
        <Layout t="layout.Vertical" Spacing="0"/>
        <Field t="fld.Field" FieldData="%!@y%" ID="Y" Title="Y">
          <FieldType t="fld_type.EditableNumber" NumberValidation="true">
            <Style t="editable_number.Slider" Width="150px"/>
            <Format t="css_dt.OffsetPx"/>
          </FieldType>
          <FieldAspect t="field_feature.Layout">
            <Css><![CDATA[#this { margin-left: 96px; }
#wrapper { }
]]></Css>
          </FieldAspect>
        </Field>
        <Field t="fld.Field" FieldData="%!@x%" ID="X" Title="X">
          <FieldType t="fld_type.EditableNumber" NumberValidation="true">
            <Style t="editable_number.Slider" Width="150px"/>
            <Format t="css_dt.OffsetPx"/>
          </FieldType>
        </Field>
        <FieldAspect t="field_aspect.Hidden">
          <OnCondition t="field_aspect.CheckConditionForEveryItem" ShowCondition="%@type% == 'xy'" DataForCondition="Item data" WhenHidden="Do not render"/>
        </FieldAspect>
      </Field>
      <Field t="control.Layout" ID="Unified" Title="Unified">
        <Layout t="layout.Vertical" Spacing="0"/>
        <Field t="fld.Field" FieldData="%!@all%" ID="Top" Title="All">
          <FieldType t="fld_type.EditableNumber" NumberValidation="true">
            <Style t="editable_number.Slider" Width="150px"/>
            <Format t="css_dt.OffsetPx"/>
          </FieldType>
        </Field>
        <FieldAspect t="field_aspect.Hidden">
          <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%@type% == 'unified'"/>
        </FieldAspect>
      </Field>
      <FieldAspect t="field_aspect.Hidden">
        <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%@error% == ''"/>
      </FieldAspect>
      <FieldAspect t="field_aspect.Title">
        <Title t="data.Pipeline">
          <Item t="xml.Tag"/>
          <Item t="text.Capitalize"/>
        </Title>
      </FieldAspect>
    </Field>
    <Field t="control.Layout" ID="Group" Title="Error">
      <Field t="control.Label" ID="error" Title="Error" Text="%@error%">
        <Style t="ui.CustomCss" base="uitext.PlainText">
          <Style t="uitext.PlainText"/>
          <Css><![CDATA[#this { text-align:center; color:#B2B2B7; width: 300px; }
]]></Css>
        </Style>
      </Field>
      <Layout t="layout.Default"/>
      <FieldAspect t="field_aspect.Hidden">
        <OnCondition t="yesno.NotEmpty" Value="%@error%" CheckInnerText=""/>
      </FieldAspect>
      <FieldAspect t="field_aspect.Title">
        <Title t="data.Pipeline">
          <Item t="xml.Tag"/>
          <Item t="text.Capitalize"/>
        </Title>
      </FieldAspect>
    </Field>
    <Layout t="layout.Default"/>
    <SectionStyle t="css_dt.CssEditor"/>
  </xtml>
</Component>

<Component id="EditorGroup" type="field.Fields">
  <Param name="CssAsXml"/>
  <Param name="Title" Default="Designer"/>
  <Param name="ID"/>
  <xtml t="control.Layout" ID="EditorGroup" Title="%$Title%">
    <Layout t="ui.CustomCss" base="layout.Default">
      <Style t="layout.Default"/>
      <Css><![CDATA[#this {
  width: 390px;
  overflow-x: hidden;
  font: Arial, Helvetica;
}
]]></Css>
    </Layout>
    <Field t="fld.Field" ID="selector" Title="Selector" FieldData="%$SelectorId%">
      <FieldType t="fld_type.Picklist" AllowEmptyValue="" AllowValueNotInOptions="">
        <Options t="editable_picklist.DynamicOptions" Options="%Css/@selector%" OptionCode="%%"/>
        <Style t="editable_picklist.JBartPicklist" Width="150px"/>
      </FieldType>
      <FieldAspect t="field_feature.HandleEvent">
        <Event t="field_feature.Update"/>
        <Action t="field.RefreshField" FireOnUpdate="" FieldID="EditorTabs" Scope="screen"/>
      </FieldAspect>
      <FieldAspect t="field_feature.Layout">
        <Css><![CDATA[#this { position:absolute; margin-top: -5px; margin-left: 230px; }
#wrapper { }
]]></Css>
      </FieldAspect>
    </Field>
    <Field t="field.TabControl" RefreshTabsOnSelect="true" ID="EditorTabs">
      <Field t="css_dt.LayoutGroup"/>
      <Field t="css_dt.TextGroup"/>
      <Field t="css_dt.ColorGroup"/>
      <Field t="css_dt.EffectsGroup"/>
      <Style t="ui.CustomCss" base="tabcontrol.Links">
        <Style t="tabcontrol.Links"/>
        <Css><![CDATA[#this >div>div.aatabs_tab { float: left; color:#002FA7; font-size: 11px; padding-top: 0; 
  padding-right: 17px; padding-bottom: 0; padding-left: 0; 
  margin-bottom: 7px; }
#this >div>div>div.aatabs_tab_text { cursor: pointer; }
#this >div>div.aa_selected_tab { font-weight:bold; text-decoration:underline; }
#this >div>div { }
#this >div>div>div.aatabs_tab_image { float: left; }
#this >.aatabs_contents { padding-top: 5px; padding-right: 0; padding-bottom: 15px; 
  padding-left: 0; border-top: 1px solid #ddd; clear: both; }
.right2left #this >div>div.aatabs_tab { float: right; padding-top: 6px; 
  padding-right: 6px; padding-bottom: 4px; 
  padding-left: 15px; }
.right2left #this >div>div>div.aatabs_tab_image { float: right; }
.right2left #this >div>div>div.aatabs_tab_text { float: right; padding-top: 0; 
  padding-right: 5px; 
  padding-bottom: 0; padding-left: 0; }
]]></Css>
      </Style>
      <FieldAspect t="field_aspect.FieldData" FieldData="%Css[@selector='{$SelectorId}']/Simplifiers%"/>
      <FieldAspect t="field_aspect.RunAction">
        <Action t="xml.RegisterModifyOnXml" SingletonId="jBStudioCssInner" Xml="%%">
          <Action t="field.RefreshField" FireOnUpdate="" FieldID="jBStudioCssInner" Scope="screen"/>
        </Action>
      </FieldAspect>
      <FieldAspect t="field_aspect.Variable" VarName="Selector" Value="%..%"/>
      <Field t="control.Layout" ID="Debug" Title="Debug">
        <Layout t="layout.Vertical" Spacing="0"/>
        <Field t="fld.Field" Title="">
          <FieldType t="fld_type.EditableText">
            <Style t="editable_text.CodeMirror" Readonly="false" Autofocus="false" LineNumbers="false" LineWrapping="false" EnableFullScreen="true" Width="400px" Height="200px" Mode="javascript">&#xa;                </Style>
          </FieldType>
          <FieldData t="xml.XmlToText" PrettyPrint="true" Data="%..%"/>
        </Field>
        <FieldAspect t="field_aspect.Hidden">
          <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render">
            <ShowCondition t="data.JavaScript" Code="function(data,html_elem,context) { return window.location.href.indexOf('?widget=CssEditor') != -1; }"/>
          </OnCondition>
        </FieldAspect>
      </Field>
    </Field>
    <FieldAspect t="field_aspect.FieldData" FieldData="%$CssAsXml%"/>
    <FieldAspect t="field_aspect.Variable" VarName="SelectorId">
      <Value t="data.WritableText" DefaultValue="%Css[1]/@selector%"/>
    </FieldAspect>
  </xtml>
</Component>

<Component id="LayoutGroup" type="field.Fields">
  <xtml t="control.Layout" ID="LayoutGroup" Title="Layout">
    <Layout t="layout.Default"/>
    <Field t="control.PropertySheet" ID="Size" Title="Size">
      <Style t="properties.TitlesAboveFields" HorizontalSpacing="15px" VerticalSpacing="8px"/>
      <Field t="fld.Field" FieldData="%!width/@value%" ID="width" Title="Width">
        <FieldType t="fld_type.EditableNumber" NumberValidation="false" AllowEmptyValue="true">
          <Style t="ui.CustomCss" base="editable_number.Slider">
            <Style t="editable_number.Slider" Width="120px"/>
            <Css><![CDATA[#this { height: 30px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; 
  padding-left: 12px; }
.aa_slider_scale { position:relative; float: left; background-color:#aaa; width: 120px; 
  height: 1px; border-radius: 3px; margin-top: 13px; margin-right: 6px; 
  margin-bottom: 13px; margin-left: 6px; 
  border-bottom: 1px solid #efefef; }
.aa_slider_thumb { cursor: pointer; position: absolute; color:black; 
  background-color:#efefef; width: 6px; height: 13px; 
  border-radius: 2px; border: 1px solid #adadad; 
  box-shadow:  inset 0 0 3px 0px          grey; top: -7px; left: 0px; }
.aa_slider_thumb:hover { background-color:#eee; border-color: #777; }
.aa_slider_scale:hover .aa_slider_text { color:black; }
.aa_slider_thumb:focus { background-color:#383838; }
.aa_slider_text { color:#aaa; font-size: 10px; margin-top: -12px; margin-right: 0px; 
  margin-bottom: 0px; margin-left: -19px; }
#this.empty_value { opacity: 0.5; -webkit-transition: opacity, 0.5s; }
.aa_slider_text:hover { cursor: pointer; text-decoration:underline; }
.aa_slider_input { position:absolute; width: 98px; height: 20px; border-radius: 6px; 
  padding-top: 4px; padding-right: 4px; padding-bottom: 4px; 
  padding-left: 18px; margin-top: -13px; margin-left: 24px; 
  border: 1px solid #D1D1D1; 
  box-shadow:  2px 2px 6px 1px       grey; }
.aa_slider_thumb.aa_disabled { opacity: 0.5; }
.aa_slider_text.aa_disabled { opacity: 0.5; }
.aa_slider_scale.aa_disabled { opacity: 0.5; }
]]></Css>
          </Style>
          <Unit t="css_dt.PositivePx"/>
          <Format t="css_dt.PositivePx">&#xa;              </Format>
        </FieldType>
        <FieldAspect t="field_aspect.DisableByCondition" DisableText="%width/@error%">
          <EnableCondition t="yesno.IsEmpty" Value="%width/@error%"/>
        </FieldAspect>
        <FieldAspect t="field_aspect.RefreshDependentFields" FieldsIds="Overflow_group" RefreshOn="every click" RefreshScope="screen"/>
      </Field>
      <Field t="fld.Field" FieldData="%!height/@value%" ID="height" Title="Height">
        <FieldType t="fld_type.EditableNumber" NumberValidation="true">
          <Style t="editable_number.Slider" Width="120px"/>
          <Format t="css_dt.PositivePx">&#xa;              </Format>
        </FieldType>
        <FieldAspect t="field_aspect.DisableByCondition" DisableText="%height/@error%">
          <EnableCondition t="yesno.IsEmpty" Value="%height/@error%"/>
        </FieldAspect>
        <FieldAspect t="field_aspect.RefreshDependentFields" FieldsIds="Overflow_group" RefreshOn="every click" RefreshScope="screen"/>
      </Field>
      <Field t="fld.Field" FieldData="%!float/@value%" ID="float" Title="Float">
        <FieldType t="fld_type.Picklist" AllowEmptyValue="true" AllowValueNotInOptions="">
          <Options t="editable_picklist.OptionsByCommas" Options="left,right"/>
          <Style t="ui.CustomCss" base="editable_picklist.JBartPicklist">
            <Style t="editable_picklist.JBartPicklist" Width="60px"/>
            <Css><![CDATA[#this { display: inline-block; }
.aa_picklist_div { cursor: arrow; overflow: hidden; text-overflow:ellipsis; 
  position: relative; white-space:absolute; 
  background:url(_jbartImages_/css/shadow2.png) repeat-x scroll 0 0 transparent; 
  text-overflow:ellipsis; font-size: 11px; width: 60px; height: 14px; 
  padding-top: 3px; padding-right: 3px; padding-bottom: 3px; 
  padding-left: 3px; border: 1px solid #BDC7D8; }
.aa_picklist_div>.aa_picklist_text { vertical-align:top; display: inline-block; }
.aa_picklist_div>.aa_picklist_image { padding-right: 5px; display: inline-block; }
.aa_picklist_div>.aa_picklist_drop_image { position: absolute; right: 20px; top: 6px; 
  display: inline-block; }
.aa_picklist_div:hover .aa_picklist_drop_image:before,
.aa_picklist_div:hover .aa_picklist_drop_image:after,
.aa_picklist_div.aa_opened_popup .aa_picklist_drop_image:before,
.aa_picklist_div.aa_opened_popup .aa_picklist_drop_image:after { 
  background:#528DD1; 
  }
.aa_picklist_drop_image:before, .aa_picklist_drop_image:after { position: absolute; 
  background:#8EB5E2; 
  width: 3px; 
  height: 8px; 
  border-radius: 2px 2px 0 0; 
  content: ""; 
  left: 10px; top: 0; 
  -moz-border-radius: 2px 2px 0 0; 
  -webkit-transform: rotate(-45deg); 
  -moz-transform: rotate(-45deg); 
  -ms-transform: rotate(-45deg); 
  -o-transform: rotate(-45deg); 
  transform: rotate(-45deg); 
  -webkit-transform-origin: 0 100\%; 
  -moz-transform-origin: 0 100\%; 
  -ms-transform-origin: 0 100\%; 
  -o-transform-origin: 0 100\%; 
  transform-origin: 0 100\%; 
  }
.aa_picklist_drop_image:after { left: 8px; -webkit-transform: rotate(45deg); 
  -moz-transform: rotate(45deg); 
  -ms-transform: rotate(45deg); 
  -o-transform: rotate(45deg); 
  transform: rotate(45deg); 
  -webkit-transform-origin: 100\% 100\%; 
  -moz-transform-origin: 100\% 100\%; 
  -ms-transform-origin: 100\% 100\%; 
  -o-transform-origin: 100\% 100\%; 
  transform-origin :100\% 100\%; }
.aa_picklist.aa_error #this .aa_picklist_div { background:#fbebeb; 
  border: 1px solid #feb2b2; }
.aa_picklist_div>.aa_picklist_text.placeholder { color:#8B8B8B; font-style:italic; }
#popup.aa_picklist_popup { background:white; padding-top: 0; padding-right: 0; 
  padding-bottom: 2px; padding-left: 0; 
  border: 1px solid gray; 
  box-shadow:  2px 2px 7px 1px rgba(50,50,50,0.5); 
  overflow-y: auto; overflow-x: hidden; font:12px times; 
  min-height:50px; max-height:200px; }
#popup.aa_picklist_popup:focus { outline: none; }
.ie #popup.aa_picklist_popup { border: 1px solid black; }
#popup.aa_picklist_popup.aa_hide_searchbox .aa_picklist_popup_input { 
  display: none; 
  }
#popup.aa_picklist_popup .aa_picklist_popup_input { font-size: 11px; width: 100\%; 
  height: 22px; padding-top: 3px; 
  padding-right: 3px; 
  padding-bottom: 3px; 
  padding-left: 3px; 
  border-bottom: 1px solid rgba(82, 168, 236, 0.8); 
  box-sizing: border-box; 
  outline: none; }
#popup.aa_picklist_popup .aa_picklist_popup_input:focus { outline: 0; }
#popup.aa_picklist_popup .aa_picklist_item { cursor: pointer; width: 100\%; 
  height: 18px; clear: both; }
#popup.aa_picklist_popup .aa_picklist_item:focus { outline: none; }
#popup.aa_picklist_popup .aa_picklist_item_image { float:left; padding-top: 1px; 
  padding-right: 5px; 
  padding-bottom: 0; 
  padding-left: 3px; }
#popup.aa_picklist_popup .aa_picklist_item_image img { height: 14px; }
#popup.aa_picklist_popup .aa_picklist_item.selected { background:#D9E8FB; }
#popup.aa_picklist_popup .aa_picklist_item_text { cursor:pointer; 
  white-space:absolute; 
  padding-top: 2px; 
  padding-right: 4px; 
  padding-bottom: 2px; 
  padding-left: 4px; }
#popup.aa_picklist_popup .aa_highlight { color:blue; }
#popup.aa_picklist_popup .aa_picklist_no_results { padding-top: 3px; 
  padding-right: 5px; 
  padding-bottom: 3px; 
  padding-left: 5px; }
#popup.aa_picklist_popup .aa_picklist_show_all { cursor: pointer; color:#333; 
  padding-top: 5px; 
  padding-right: 5px; 
  padding-bottom: 5px; 
  padding-left: 5px; }
]]></Css>
          </Style>
        </FieldType>
      </Field>
    </Field>
    <Field t="control.Layout" ID="Overflow_group" Title="Group">
      <Field t="control.PropertySheet" ID="Overflow_properties" Title="Overflow">
        <Style t="properties.TitlesAboveFields" HorizontalSpacing="25px" VerticalSpacing="10px"/>
        <Field t="fld.Field" FieldData="%!overflow_x/@value%" ID="overflow_x" Title="Overflow X">
          <FieldType t="fld_type.Picklist" AllowEmptyValue="true" AllowValueNotInOptions="true">
            <Options t="editable_picklist.OptionsByCommas" Options="visible,hidden,scroll,auto"/>
            <Style t="editable_picklist.JBartPicklist" Width="70px"/>
          </FieldType>
        </Field>
        <Field t="fld.Field" FieldData="%!overflow_y/@value%" ID="overflow_y" Title="Overflow Y">
          <FieldType t="fld_type.Picklist" AllowEmptyValue="true" AllowValueNotInOptions="true">
            <Options t="editable_picklist.OptionsByCommas" Options="visible,hidden,scroll,auto"/>
            <Style t="editable_picklist.JBartPicklist" Width="70px"/>
          </FieldType>
        </Field>
        <FieldAspect t="field_aspect.Hidden">
          <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render">
            <ShowCondition t="yesno.OR">
              <Item t="yesno.NotEmpty" Value="%width/@value%"/>
              <Item t="yesno.NotEmpty" Value="%height/@value%"/>
            </ShowCondition>
          </OnCondition>
        </FieldAspect>
        <Field t="fld.Field" FieldData="%!overflow/@value%" ID="overflow" Title="Overflow">
          <FieldType t="fld_type.Picklist" AllowEmptyValue="true" AllowValueNotInOptions="true">
            <Options t="editable_picklist.OptionsByCommas" Options="visible,hidden,scroll,auto"/>
            <Style t="editable_picklist.JBartPicklist" Width="70px"/>
          </FieldType>
