            <Style t="editable_picklist.JBartPicklist" Width="77px"/>
          </FieldType>
        </Field>
        <Field t="fld.Field" FieldData="%!border_color/@value%" ID="color" Title="Color">
          <FieldType t="fld_type.EditableColor">
            <Style t="ui.CustomCss" base="editable_color.JSColor">
              <Style t="editable_color.JSColor"/>
              <Css><![CDATA[#this { font-size: 11px; width: 70px; height: 16px; padding-top: 3px; padding-right: 3px; 
  padding-bottom: 3px; padding-left: 3px; border: 1px solid #BDC7D8; }
#this:focus { 
