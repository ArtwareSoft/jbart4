            </Format>
          </FieldType>
          <FieldAspect t="field_feature.HandleEvent">
            <Event t="field_feature.Update"/>
            <Action t="action.IfThenElse">
              <If t="yesno.And">
                <Item t="yesno.IsEmpty" Value="%../../border_style/@value%"/>
              </If>
              <Then t="action.RunActions">
                <Action t="action.WriteValue" To="%!../../border_style/@value%" Value="solid"/>
                <Action t="field.RefreshField" FireOnUpdate="" FieldID="border_style" Scope="screen"/>
              </Then>
            </Action>
          </FieldAspect>
        </Field>
        <Field t="fld.Field" FieldData="%!border_style/@value%" ID="border_style" Title="Style">
          <FieldType t="fld_type.Picklist" AllowEmptyValue="true" AllowValueNotInOptions="">
