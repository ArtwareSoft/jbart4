<xtml ns="xtml_dt2" light="false"><Component id="UseXtmlDt2" type="data.Boolean">  <xtml1 t="yesno.EqualsTo" To="true">      <Data t="ui.UrlFragment" Attribute="xtmldt2"/>  </xtml1>  <xtml t="yesno.Not">    <Of t="yesno.EqualsTo" To="false">      <Data t="ui.UrlFragment" Attribute="xtmldt2"/>    </Of>  </xtml></Component><Component id="XtmlEditor" type="field.Fields">  <Param name="Xtml" Default="%%"/>  <xtml t="control.Layout" ID="Xtml_Editor_all" Title="Xtml Editor All">    <Var name="_ShowAdvancedParams" varScope="Component" t="data.WritableText" DefaultValue="false"/>    <Field t="control.PropertySheet" ID="Xtml_Editor" Title="Xtml Editor basic">      <Style t="gstudio.XtmlDtPropertySheet"/>      <Field t="data.Pipeline">        <Item t="xtml_dt2.ComponentParams" Xtml="%$Xtml%" BasicOrAdvanced="basic"/>        <Item t="xtml_dt2.ProfileParamEditor" Xtml="%$Xtml%" ParamXml="%%"/>      </Field>      <FieldAspect t="field_aspect.FieldData" FieldData="%$Xtml%"/>    </Field>    <Field t="control.Button" ID="xtmldt_more_button" Title="more..." HideTitle="true">      <Style t="btn.Hyperlink"/>      <FieldAspect t="field_feature.Layout" dragProperty="#this margin">        <Css><![CDATA[#this { margin-top: 6px; margin-left: 0; }]]></Css>      </FieldAspect>      <Action t="action.RunActions">        <Action t="action.ToggleBooleanValue" To="%$_ShowAdvancedParams%"/>        <Action t="field.RefreshField" FireOnUpdate="" FieldID="Xtml_Editor_more,xtmldt_more_button" Scope="sibling"/>      </Action>      <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Do not render">        <ShowCondition t="yesno.And">          <Item value="%$_ShowAdvancedParams% != 'true'"/>          <Item t="yesno.NotEmpty">            <Value t="xtml_dt2.ComponentParams" Xtml="%$Xtml%" BasicOrAdvanced="advanced"/>          </Item>        </ShowCondition>      </FieldAspect>    </Field>    <Field t="control.PropertySheet" ID="Xtml_Editor_more" Title="Xtml Editor more">      <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Render as hidden html" ShowCondition="%$_ShowAdvancedParams%"/>      <Style t="gstudio.XtmlDtPropertySheet"/>      <Field t="data.Pipeline">        <Item t="xtml_dt2.ComponentParams" Xtml="%$Xtml%" BasicOrAdvanced="advanced"/>        <Item t="xtml_dt2.ProfileParamEditor" Xtml="%$Xtml%" ParamXml="%%"/>      </Field>      <FieldAspect t="field_aspect.FieldData" FieldData="%$Xtml%"/>      <FieldAspect t="field_feature.Layout" dragProperty="#this margin">        <Css><![CDATA[#this { margin-top: 6px; margin-left: 0; }]]></Css>      </FieldAspect>    </Field>    <Layout t="layout.Default"/>    <Field t="control.PropertySheet" ID="Variables" Title="Variables">      <Condition t="yesno.NotEmpty" Value="%$Xtml/Var%"/>      <Style t="gstudio.XtmlDtPropertySheet"/>      <Field t="field.DynamicFields" ID="_Fields_from_metadata__Dynamic_Fields_" Title="vars" FieldItems="%$Xtml/Var%">        <Field t="control.Button" ID="xtmldt_var_button" Title="var" HideTitle="false" ButtonText="edit in aaeditor...">          <Style t="ui.CustomCss" base="btn.Hyperlink">            <Style t="btn.Hyperlink"/>            <Css><![CDATA[#this {  cursor:pointer;  font: 12 arial;  padding-top: 4px;  color:blue;  font-size:11px;  text-decoration:none;  display:inline-block;}#this:hover {  text-decoration:underline;}#this.disabled {  text-decoration:none;  cursor:default;}]]></Css>          </Style>          <Action t="gstudio_popup.ShowAAEditorFloatingPopup" Xtml="%$_FieldItem%" Title="AAEditor">            <Title value="AAEditor"/>          </Action>          <FieldAspect t="field_aspect.Title" Title="Variable (%$_FieldItem/@name%)"/>        </Field>      </Field>      <FieldAspect t="field_feature.Layout" dragProperty="#this margin">        <Css><![CDATA[#this { margin-top: 8px; margin-left: 0; }#wrapper { }]]></Css>      </FieldAspect>    </Field>  </xtml></Component>  <Component id="ComponentParams" type="data.Data" execution="native">    <Param name="Xtml" />    <Param name="BasicOrAdvanced" type="enum" Options="basic,advanced" Default="basic" />  </Component>  <Component id="ParamPTOfEditor" type="data.Data" execution="native">    <Param name="Xtml" />    <Param name="ParamXml" />    <NativeHelper name="ForCompress" t="data.List">      <Item t="xtml_dt2.CssParamEditor" />      <Item t="xtml_dt2.PrimitiveParamEditor" />      <Item t="xtml_dt2.EnumParamEditor" />      <Item t="xtml_dt2.MultiEnumParamEditor" />      <Item t="xtml_dt2.BooleanParamEditor" />      <Item t="xtml_dt2.SliderParamEditor" />      <Item t="xtml_dt2.TgpArrayParamEditor" />      <Item t="xtml_dt2.TgpTypeParamEditor" />    </NativeHelper>  </Component>	<Component id="ProfileParamEditor" type="field.Fields" dtsupport="false" execution="native">		<Param name="Xtml" />		<Param name="ParamXml" />    <NativeHelper name="TypeOfParam" t="xtml_dt2.ParamPTOfEditor" Xtml="%$Xtml%" ParamXml="%$ParamXml%" />    <NativeHelper name="FieldAspects" t="field_aspect.Aspects">      <FieldAspect t="xtml_dt2.DefaultProfileParamAspects" Xtml="%%" ParamXml="%$ParamXml%" />      <FieldAspect t="field_aspect.HideTitle" Condition="%$ParamXml/@hideTitle%" />      <FieldAspect t="field_aspect.Description" Description="%$ParamXml/@description%" />      <FieldAspect t="field_feature.HandleEvent" Condition="%$ParamXml/@name% == 'Title'" >        <Event t="field_feature.Update" />        <Action t="action.RunActions">          <Action t="cntr.RefreshSelectedTextAndImage">            <Cntr t="cntr.ContainerOfElement" RunOn=".jbstudio_tree" />          </Action>          <Action1 t="action.RunActions" Condition="%$_StudioIsNewItem%" >            <Var name="NewID" t="text.ToId" Text="%@Title%" UsedIDs="%../@ID%" />             <Action t="action.WriteValue" To="%@ID%" Value="%$NewID%" />            <Action t="field.RefreshField" FieldID="xtml_dt_ID" Scope="screen" />             <Action t="action.RunActions">              <Action t="action.WriteValue" To="%@FieldData%" Value="\%@%$NewID%\%" />              <Action t="field.RefreshField" FieldID="xtml_dt_FieldData" Scope="screen" />            </Action>           </Action1>        </Action>      </FieldAspect>              <FieldAspect t="data.Pipeline">        <Item value="%$ParamXml/EditorFieldAspect%" />        <Item t="xtml.RunXtml" Xtml="%%">          <Var name="Xtml" value="%$Xtml%" />        </Item>      </FieldAspect>    </NativeHelper>	</Component>	<Component id="PrimitiveParamEditor" type="field.Fields" >		<Param name="Xtml" />		<Param name="ParamXml" />		<xtml t="control.Layout" ID="primitive_editor" Title="">			<Field t="data.FirstSucceeding">			  <Item t="xtml_dt2.PrimitiveParamEditorAsTextbox" Xtml="%$Xtml%" ParamXml="%$ParamXml%">          <Condition t="xtml_dt2.IsParamPrimitiveValue" Xtml="%$Xtml%" ParamXml="%$ParamXml%" />        </Item>        <Item t="xtml_dt2.PrimitiveParamEditorAsJavascript" Xtml="%$Xtml%" ParamXml="%$ParamXml%" Condition="%$Xtml/{$ParamXml/@name}/@t% == 'js.JavaScript'" />			  <Item t="xtml_dt2.PrimitiveParamEditorAsXtml" Xtml="%$Xtml%" ParamXml="%$ParamXml%" />			</Field>		</xtml>	</Component><Component id="PrimitiveParamEditorAsXtml" type="field.Fields">  <Param name="Xtml"/>  <Param name="ParamXml"/>  <xtml t="control.Layout" ID="xtmldt_computed" Title="computed">    <Layout t="layout.Horizontal" Spacing="0"/>    <Field t="control.Button" ID="xtmldt_computed_button" Title="computed" HideTitle="true">      <Style t="gstudio.XtmlDTOpenAAEditor"/>      <Action t="gstudio_popup.ShowAAEditorFloatingPopup" Xtml="%$Xtml/{$ParamXml/@name}%"/>      <FieldAspect t="field_aspect.Title" Title="(...)"/>    </Field>    <Field t="control.Button" ID="primitive_editor_dropdown" Title="Menu" HideTitle="true">      <Style t="gstudio.PrimitiveTypeArrow"/>      <FieldAspect t="popup_field_aspect.ToolbarPopup">        <PopupStyle t="ui.CustomCss" base="popup.CascadingMenu">          <Style t="popup.CascadingMenu"/>          <Css><![CDATA[#this>.aa_popup_frame {  background:#fff;  min-width:150px;  min-height:30px;  box-shadow:2px 2px 3px #d5d5d5;  padding:3px 0;  border:1px solid rgb(213, 213, 213);}#this>.aa_popup_frame>.aa_popup_contents {} #this>.aa_popup_frame:focus {  outline:none;}.aa_popup_remove_top_border {  display:none;}]]></Css>        </PopupStyle>        <Contents t="field.InnerPage">          <Layout t="layout.Default"/>          <Field t="control.Button" ID="_Button3" Title="Edit in AAEditor..." HideTitle="true">            <Image t="gstudio.StudioSmallImage" PositionInSprite="240,0"/>            <Style t="gstudio.StudioMenuItem"/>            <Action t="action.RunActions">              <Action t="xml.AddChildren" Data="%$Xtml%">                <Children t="xml.ElementOfDynamicTag" Tag="%$ParamXml/@name%">                  <Change t="xml.SetAttribute" AttributeName="t">                    <Value t="data.IfThenElse" Else="data.Pipeline" Then="yesno.And" If="%$ParamXml/@type%=='data.Boolean'"/>                  </Change>                  <Change t="xml.AddChildren">                    <Children t="xml.Xml" DynamicContent="true">                      <Item value="%$Xtml/@{$ParamXml/@name}%"/>                    </Children>                  </Change>                </Children>              </Action>              <Action t="xml.Delete" Data="%$Xtml/@{$ParamXml/@name}%"/>              <Action t="field.RefreshField" FireOnUpdate="" FieldID="xtml_dt_%$ParamXml/@name%" Scope="sibling">                <Var name="ControlElement" value="%$PopupLaunchingElement%"/>              </Action>              <Action t="gstudio_popup.ShowAAEditorFloatingPopup">                <Xtml t="data.FirstSucceeding">                  <Item value="%$Xtml/{$ParamXml/@name}%"/>                  <Item value="%$Xtml%"/>                </Xtml>              </Action>            </Action>          </Field>          <Field t="control.Button" ID="_Button4" Title="Delete script" HideTitle="true">            <Style t="gstudio.StudioMenuItem"/>            <Action t="action.RunActions">              <Action t="xml.ChangeXml" Xml="%$Xtml/{$ParamXml/@name}%">                <Change t="xml.Delete"/>              </Action>              <Action t="field.RefreshField" FireOnUpdate="" FieldID="xtml_dt_%$ParamXml/@name%" Scope="sibling" RunOn="%$PopupLaunchingElement%"/>            </Action>          </Field>        </Contents>      </FieldAspect>    </Field>  </xtml></Component><Component id="PrimitiveParamEditorAsJavascript" type="field.Fields">  <Param name="Xtml"/>  <Param name="ParamXml"/>  <xtml t="control.Layout" ID="xtmldt_js" Title="js">    <Layout t="layout.Horizontal" Spacing="0"/>    <Field t="control.Button" ID="xtmldt_js_button" Title="(javascript)" HideTitle="true">      <Style t="gstudio.XtmlDTOpenJavascript"/>      <Action t="xtml_dt_tree.OpenCodeMirror" Data="%$Xtml/{$ParamXml/@name}%">        <Var name="ParamXml" t="xtml.ToParamDefinition" Component="%$Xtml/{$ParamXml/@name}/@t%" Param="Javascript"/>      </Action>    </Field>    <Field t="control.Button" ID="_Button3" Title="Delete script" HideTitle="true" Tooltip="Delete script">      <Style t="btn.ImageOnlyButton"/>      <Image t="img.Image" Url="_jbartImages_/delete1616.gif">        <AdjustSize t="img.Center" RightToLeft=""/>      </Image>      <Action t="action.RunActions">        <Action t="xml.ChangeXml" Xml="%$Xtml/{$ParamXml/@name}%">          <Change t="xml.Delete"/>        </Action>        <Action t="field.RefreshField" FireOnUpdate="" FieldID="xtml_dt_%$ParamXml/@name%" Scope="sibling"/>      </Action>      <FieldAspect t="field_feature.Layout" dragProperty="#wrapper padding">        <Css><![CDATA[#wrapper {  padding-top:0px;  padding-left:3px;    width: 16px;  display: block;}#this {   display:none;}.fld_xtmldt_js:hover #this {  display:block;}]]></Css>      </FieldAspect>    </Field>  </xtml></Component><Component id="PrimitiveParamEditorAsTextbox" type="field.Fields">  <Param name="Xtml"/>  <Param name="ParamXml"/>  <xtml t="control.Layout" Title="" ID="paramEditorAsTextbox">    <Var name="PrimitiveValue" varScope="Component" t="data.WritableText">      <DefaultValue t="text.FirstSucceeding">        <Item value="%$Xtml/@{$ParamXml/@name}%" />        <Item value="%$ParamXml/@Default%" />      </DefaultValue>    </Var>    <Var name="HasDefaultValue" varScope="Component" t="yesno.OR">      <Item value="%$ParamXml/@Default% != ''"/>      <Item t="yesno.NotEmpty" Value="%$ParamXml/Default%"/>    </Var>    <FieldAspect t="field_aspect.Variable" VarName="XtmlAutoSuggest">      <Value t="xtml_dt2.XtmlAutoSuggestObject" Xtml="%$Xtml%" ParamXml="%$ParamXml%"/>    </FieldAspect>    <Field t="control.Layout" Title="">      <Layout t="layout.Horizontal" Spacing="0"/>      <Field t="fld.Field" FieldData="%$PrimitiveValue%" ID="prtimitive_editor_textbox" Title="Textbox">        <FieldType t="fld_type.EditableText">          <Style t="gstudio.XtmlDTTextbox"/>        </FieldType>        <FieldAspect t="xtml_dt2.PrimitiveTextboxAutoSuggestions" Xtml="%$Xtml%" ParamXml="%$ParamXml%" EditInLargePopup="true"/>        <FieldAspect t="field_feature.HandleEvent">          <Event t="field_feature.Update"/>          <Action t="action.RunActions">            <Action t="xml.SetAttribute" Data="%$Xtml%" AttributeName="%$ParamXml/@name%" Value="%$PrimitiveValue%" RemoveEmptyAttribute="%$HasDefaultValue% != 'true'"/>            <Action t="xtml_dt2.FireFieldOnUpdate"/>          </Action>        </FieldAspect>        <FieldAspect t="field_feature.HandleEvent">          <Event t="field_feature.EnterPressed"/>          <Action t="field_dt.RefreshPreview">            <RefreshParent t="yesno.And">              <Item value="%$ParamXml/@name% == 'Title'"/>              <Item value="%$Xtml/../@t% == 'control.PropertySheet' || %$Xtml/../@t% == 'control.TabControl' || %$Xtml/../@t% == 'field.ItemList'"/>            </RefreshParent>          </Action>        </FieldAspect>      </Field>      <Field t="control.Button" ID="primitive_editor_dropdown" Title="Menu" HideTitle="true">        <Style t="gstudio.PrimitiveTypeArrow"/>        <FieldAspect t="popup_field_aspect.ToolbarPopup">          <PopupStyle t="ui.CustomCss" base="popup.CascadingMenu">            <Style t="popup.CascadingMenu"/>            <Css><![CDATA[#this>.aa_popup_frame {  background:#fff;  min-width:150px;  min-height:30px;  box-shadow:2px 2px 3px #d5d5d5;  padding:3px 0;  border:1px solid rgb(213, 213, 213);}#this>.aa_popup_frame>.aa_popup_contents {} #this>.aa_popup_frame:focus {  outline:none;}.aa_popup_remove_top_border {  display:none;}]]></Css>          </PopupStyle>          <Contents t="field.InnerPage">            <Layout t="layout.Default"/>            <Field t="control.Button" ID="_Button3" Title="Edit in AAEditor..." HideTitle="true">              <Image t="gstudio.StudioSmallImage" PositionInSprite="240,0"/>              <Style t="gstudio.StudioMenuItem"/>              <Action t="action.RunActions">                <Action t="xml.AddChildren" Data="%$Xtml%">                  <Children t="xml.ElementOfDynamicTag" Tag="%$ParamXml/@name%">                    <Change t="xml.SetAttribute" AttributeName="t">                      <Value t="data.IfThenElse" Else="data.Pipeline" Then="yesno.And" If="%$ParamXml/@type%=='data.Boolean'"/>                    </Change>                    <Change t="xml.AddChildren">                      <Children t="xml.Xml" DynamicContent="true">                        <Item value="%$Xtml/@{$ParamXml/@name}%"/>                      </Children>                    </Change>                  </Children>                </Action>                <Action t="xml.Delete" Data="%$Xtml/@{$ParamXml/@name}%"/>                <Action t="field.RefreshField" FireOnUpdate="" FieldID="xtml_dt_%$ParamXml/@name%" Scope="sibling">                  <Var name="ControlElement" value="%$PopupLaunchingElement%"/>                </Action>                <Action t="gstudio_popup.ShowAAEditorFloatingPopup">                  <Xtml t="data.FirstSucceeding">                    <Item value="%$Xtml/{$ParamXml/@name}%"/>                    <Item value="%$Xtml%"/>                  </Xtml>                </Action>              </Action>            </Field>            <Field t="control.Button" ID="_Button4" Title="Javascript..." HideTitle="true"><![CDATA[]]>              <Style t="gstudio.StudioMenuItem"/>              <Action t="action.RunActions">                <Action t="xml.RemoveXmlAttribute" Element="%$Xtml%" AttributeName="%$ParamXml/@name%"/>                <Action t="xml.RemoveXmlNode" Xml="%$Xtml/{$ParamXml/@name}%"/>                <Action t="action.WriteValue" Value="js.JavaScript" To="%$Xtml/!{$ParamXml/@name}/@t%"/>                <Action t="action.WriteValue" Value="function(data,html_elem,context) {&#xa;  &#xa;}">                  <To t="xml.CDataRef" Element="%$Xtml/!{$ParamXml/@name}/Javascript%"/>                </Action>                <Action t="field.RefreshField" FireOnUpdate="" FieldID="xtml_dt_%$ParamXml/@name%" Scope="sibling">                  <Var name="ControlElement" value="%$PopupLaunchingElement%"/>                </Action>                <Action t="xtml_dt_tree.OpenCodeMirror" Data="%$Xtml/{$ParamXml/@name}%">                  <Var name="ParamXml" t="xtml.ToParamDefinition" Component="%$Xtml/{$ParamXml/@name}/@t%" Param="Javascript"/>                </Action>              </Action>            </Field>          </Contents>        </FieldAspect>      </Field>    </Field>    <Layout t="layout.Default"/>    <Field t="control.Label" ID="xtml_expression_preview" Title="Expression Preview" Text="">      <Style t="ui.CustomCss" base="uitext.PlainText">        <Style t="uitext.PlainText"/>        <Css><![CDATA[#this {  width:165px;  overflow:hidden;  color:#7d7d7d;  font-size:11px;  white-space: nowrap;  }]]></Css>      </Style>      <FieldAspect t="field_aspect.Title" Title=""/>    </Field>  </xtml></Component>	<Component id="DefaultProfileParamAspects" type="field.FieldAspect">		<Param name="Xtml" />		<Param name="ParamXml" />		<xtml t="field_aspect.Aspects">			<FieldAspect t="field_aspect.Title">        <Title t="text.Capitalize" Mode="capital to separate words" Data="%$ParamXml/@name%" />      </FieldAspect>			<FieldAspect t="field_aspect.ChangeFieldID" ID="xtml_dt_%$ParamXml/@name%"/>		</xtml>	</Component>	<Component id="IsParamPrimitiveValue" type="data.Boolean" execution="native">		<Param name="Xtml" />		<Param name="ParamXml" />	</Component><Component id="TgpTypeParamEditor" type="field.Fields">  <Param name="Xtml"/>  <Param name="ParamXml"/>  <xtml t="control.Layout" ID="primitive_editor_tgptype" Title="Tgp type">    <Var name="ValueOfT" varScope="Component" t="data.WritableText" DefaultValue="%$Xtml/{$ParamXml/@name}/@t%"/>    <Var name="_IsTgpExpanded" varScope="Component" t="data.WritableText">      <DefaultValue t="data.IfThenElse" Then="true" Else="false">        <If t="data.Pipeline">          <Item t="xtml.ComponentDefinition" ID="%$ValueOfT%"/>          <Item value="%@autoexapnd%"/>        </If>      </DefaultValue>    </Var>    <Field t="control.Layout" ID="tgptype_line" Title="line">      <Field t="fld.Field" FieldData="%$ValueOfT%" ID="prtimitive_editor_picklist" Title="Picklist">        <FieldType t="fld_type.Picklist" AllowEmptyValue="true" AllowValueNotInOptions="false">          <Options t="editable_picklist.DynamicOptions" OptionCode="%code%" OptionDisplayName="%text%">            <Options t="data.Pipeline">              <Item t="field_dt.ComponentsOfTypeOptions" Type="%$ParamXml/@type%" CurrentPT="%$Xtml/{$ParamXml/@name}/@t%"/>              <Item value="%Options%"/>            </Options>          </Options>          <Style t="gstudio.XtmlDtTgpPicklist"/>        </FieldType>        <FieldAspect t="field_feature.HandleEvent">          <Event t="field_feature.Update"/>          <Action t="action.RunActions">            <Action t="xtml_dt2.SetTgpTypeValue" Xtml="%$Xtml%" ParamXml="%$ParamXml%" Value="%%"/>            <Action t="xtml_dt.CleanXtmlOfParams" Data="%$Xtml/{$ParamXml/@name}%" PrevComponent="%$OldValue%"/>            <Action t="action.WriteValue" To="%$_IsTgpExpanded%" Value="true"/>            <Action t="field.RefreshField" FireOnUpdate="" FieldID="tgptype_editor_fields,xtmldt_picklist_buttons" Scope="sibling"/>            <Action t="field_dt.RefreshPreview"/>          </Action>        </FieldAspect>        <FieldAspect t="fld_aspect.PicklistProperties" AutoRecalcOptions="" HideSearchBox="">          <SelectOnHover t="yesno.OR">            <Item value="%$ParamXml/@name% == 'Style'"/>            <Item value="%$ParamXml/@refreshOnHover%"/>          </SelectOnHover>        </FieldAspect>      </Field>      <Layout t="layout.Horizontal" Spacing="3"/>      <Field t="control.Layout" ID="xtmldt_picklist_buttons" Title="buttons">        <Field t="control.Button" ID="xtml_dt_toggle_collapse" Title="ToggleExpandCollapse" HideTitle="true">          <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="expanded" ConditionForClass="%$_IsTgpExpanded%"/>          <Style t="gstudio.ExpandCollapseButton"/>          <Action t="action.RunActions">            <Action t="action.ToggleBooleanValue" To="%$_IsTgpExpanded%"/>            <Action t="field.RefreshField" FireOnUpdate="" FieldID="tgptype_editor_fields,xtml_dt_toggle_collapse" Scope="sibling"/>          </Action>          <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Render as hidden html"> 