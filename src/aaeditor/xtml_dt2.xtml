ng" t="popup.OpenPopup">    <Style t="popup.PicklistPopup"/>    <Feature t="popup.UniquePopup" UniqueID="xtmldt_suggest_popup"/>    <Contents t="field.InnerPage">      <Layout t="layout.Default"/>      <Field t="field.ItemList" Title="items" ID="xtmldt_suggest">        <FieldAspect t="field_aspect.ItemListContainer">          <Items t="itemlist.Items" Items="%$XtmlAutoSuggest/SuggestOptions%"/>        </FieldAspect>        <FieldAspect t="itemlist_aspect.ShowTextWhenNoItems"/>        <View t="itemlist_style.JBartTableNoHeader" MinWidth="100%"/>        <Field t="control.Label" ID="_Label" Title="Expression" Text="%Expression%">          <Style t="uitext.PlainText"/>        </Field>        <Field t="control.Label" ID="_Label1" Title="Preview" Text="%Preview%">          <Style t="ui.CustomCss" base="uitext.PlainText">            <Style t="uitext.PlainText"/>            <Css><![CDATA[#this {  font-style:italic;}]]></Css>          </Style>        </Field>        <FieldAspect t="itemlist_aspect.ItemSelection" KeyboardSupport="true" AlwaysOneSelected="true" MouseSupport="mouse hover">          <Css><![CDATA[{ background: #D9E8FB; }]]></Css>        </FieldAspect>        <FieldAspect t="itemlist_aspect.ItemClick">          <Css><![CDATA[#this:hover { cursor: pointer; } ]]></Css>          <OnClick t="action.RunActions">            <Action t="popup.CloseContainingPopup"/>            <Action t="object.RunMethod" Object="%$XtmlAutoSuggest%" Method="Select" Input="%Expression%"/>          </OnClick>        </FieldAspect>        <FieldAspect t="field_feature.Css">          <Css><![CDATA[#this {  max-height:300px;  max-width:250px;  overflow:auto;} ]]></Css>        </FieldAspect>      </Field>    </Contents>  </NativeHelper>  <NativeHelper name="SuggestionsTreeWhileTexting" t="popup.OpenPopup">    <Style t="popup.PicklistPopup"/>    <Feature t="popup.UniquePopup" UniqueID="xtmldt_suggest_popup"/>    <Contents t="field.InnerPage">      <Layout t="layout.Default"/>      <Field t="itemtree.Tree" ID="_Tree" Title="Tree" ItemSelection="false">        <Style t="itemtree.XmlTree"/>        <Items t="itemtree.TreeItems" Items="%$XtmlAutoSuggest/FirstInput%" SubItems="%*%"/>        <ItemText t="xml.Tag"/>        <ItemImage t="image.ImageInSprite" Url="_jbartImages_/studio/studio_icons.png?ver=28_09_14_2" PositionInSprite="80,32"/>        <FieldAspect t="itemtree_aspect.TreeProperties">          <ControlForItem t="field.InnerPage" Title="tree xml element">            <Field t="xmltree.XmlElementTreeItem" ID="xml_tree_elem" Title="tree element">              <Style t="xmltree_style.Default"/>              <OnClick t="action.RunActions">                <Action t="popup.CloseContainingPopup"/>                <Action t="object.RunMethod" Object="%$XtmlAutoSuggest%" Method="Select">                  <Input t="xml.XPathOfNode" TopXml="%$XtmlAutoSuggest/FirstInput%"/>                </Action>              </OnClick>            </Field>          </ControlForItem>        </FieldAspect>        <FieldAspect t="field_feature.Css">          <Css><![CDATA[#this { width: 300px; height: 150px; overflow-x: auto; overflow-y: auto; padding: 5px; }]]></Css>        </FieldAspect>        <FieldAspect t="itemtree_aspect.TreeLongList" MaxItemsPerPage="25" TextForShowMore="More ..."/>      </Field>    </Contents>  </NativeHelper></Component><Component id="FireFieldOnUpdate" type="action.Action" execution="native" /><Component id="JavaScriptEditorField" type="field.Fields">    <xtml t="control.Button" ID="xtmldt_js_button" Title="Javascript" HideTitle="true" ButtonText="(...)">      <Style t="gstudio.XtmlDTOpenJavascript"/>      <Action t="xtml_dt_tree.OpenCodeMirror">        <Var name="ParamXml" t="xtml.ToParamDefinition" Component="js.JavaScript" Param="Javascript"/>      </Action>    </xtml>  </Component></xtml>