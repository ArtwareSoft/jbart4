<xtml ns="xtml_field_tree" light="false"> <Component id="FieldTreeSubItems" type="data.Data" execution="native" /> <Component id="FieldTreeItemText" type="data.Data">    <xtml t="text.FirstSucceeding">      <Item value="%@Title%" />      <Item value="%@ID%" />      <Item value="%../@id%" />      <Item value="%@t%" />    </xtml> </Component><Component id="FieldTree" type="field.Fields">  <Param name="FieldXtml"/>  <xtml t="control.Layout" ID="_Vertical_Layout" Title="Vertical Layout">    <Field t="itemtree.Tree" ID="_Tree" Title="Tree" ItemSelection="true">      <Style t="gstudio.XtmlFieldTreeStyle"/>      <Items t="itemtree.TreeItems">        <Items t="field_dt.UpToPageDef" Data="%$FieldXtml%"/>        <SubItems t="xtml_field_tree.FieldTreeSubItems"/>      </Items>      <ItemText t="xtml_field_tree.FieldTreeItemText"/>      <ItemImage t="image.ImageInSprite" Url="_jbartImages_/studio/studio_icons.png?ver=18_09_14_6">        <PositionInSprite t="text.FirstSucceeding">          <Item value="16,80" Condition="%@t% == 'field.InnerPage'"/>          <Item value="32,80" Condition="%@t% == 'control.Layout'"/>          <Item value="0,80"/>        </PositionInSprite>      </ItemImage>    </Field>  </xtml></Component></xtml>