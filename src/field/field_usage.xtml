ds t="field.Fields">          <Field t="field.XmlField" Path="@num" ID="num" Title="Num">            <Type t="field_aspect.Text"/>            <FieldAspect t="field_aspect.Validation" CheckValidation="on blur" ErrorMessage="should be smaller than const">              <ValidationLogic t="validation.CustomValidation">                <SuccessCondition t="yesno.GreaterThan" Than="%%" Item="%../@const%" />              </ValidationLogic>            </FieldAspect>          </Field>          <Field t="field.XmlField" Path="@const" ID="const" Title="Const (calculated)">            <Type t="field_aspect.Text"/>            <FieldAspect t="field_aspect.Description" Description="calculated field"/>            <FieldAspect t="field_aspect.Calculated" ManualOverride="" MathFormula="" PrimitiveField="true" Formula="5"/>          </Field>        </Fields>      </Control>      <ExpectedResult t="yesno.NotContains">        <Text value="should be smaller than"/>      </ExpectedResult></Usage><Usage t="ui.ControlUsage" Of="field_aspect.CustomSuggestionBox">      <Var name="Countries" t="xml.Xml">        <xml>          <category name="Africa" desc="wildest continent">            <category name="West Africa">              <country name="Gana" Capital="Zoluta"/>            </category>            <category name="East Africa">              <country name="Kenya" Capital="Nairobi"/>            </category>          </category>          <category name="America">			</category>        </xml>      </Var>      <Var name="Text" t="xml.Xml"> <xml text="" /></Var>      <Data value="i like %Africa"/>      <Control t="ui.List">      	<Control t="ui.Document">          <Item t="data_items.Items" Items="%$Text%"/>          <Fields t="field.XmlField" Path="@text">              <FieldAspect t="field_aspect.CustomSuggestionBox" Name="suggestion_box" CssStyle="width:300px">				          <OnChangeAtEndOfText t="action.IfThenElse" >				          	<Var name="XPath" t="text.Concat" ItemText="*[@name='%%']" Separator="/" >				                <Items t="data.Pipeline">				                  <Item t="text.Split" Separator="%" Part="Last"/>				                  <Item t="text.Split" Separator="/" Part="All but Last"/>				                </Items>				            </Var>				            <Var name="Item" t="xml.XPath" From="%$Countries%" XPath="%$XPath%" />				            <If t="yesno.NotEmpty" Value="%$Item/*%"/>				            <Condition t="yesno.Contains" AllText="%%">				            	<Text value="\%" />				            </Condition>				            <Then t="field_aspect.OpenSuggestionBoxList" Options="%$Item/*%" OptionLabel="%@name%" OptionDescription="%@desc%"/>				            <Else t="field_aspect.OpenSuggestionBoxPopup">				              <Control t="xmlui.XmlSelector" Data="%$Item%">				                <OnSelect t="field_aspect.AddTextToSuggestionBox"/>				              </Control>				            </Else>				          </OnChangeAtEndOfText>				       </FieldAspect>          </Fields>          </Control>        <Control t="ui.ButtonOld" Text="From Africa">          <OnClick t="field_aspect.TriggerSuggestionBoxPopup" TextToSimulate="\%Africa/" TextPatternToAdd="\%Africa/%%" RunOn=".fld_text"/>        </Control>      </Control>      <CleanAfter t="field_aspect.CloseSuggestionBox" RunOn=".fld_text"/>      <RunOnControl t="action.RunActions" RunOn=".fld_text">        <Action t="uiaction.Focus"/>        <Action t="uiaction.SetText" Text="i like \%Africa/"/>        <Action t="field_aspect.TriggerSuggestionBoxPopup" TextToSimulate="i like \%Africa/" TextPatternToAdd="%%" />      </RunOnControl>      <ExpectedResult t="yesno.Contains" RunOn=".suggestionpopup">        <Data t="ui.ControlToRunOn"/>        <Text value="West Africa"/>      </ExpectedResult>    </Usage></xtml> 