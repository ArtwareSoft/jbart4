<xtml ns="field_usage">  <Usage t="ui.ControlUsage" Of="field.RefrehsField.Table">      <Var name="Items" t="xml.Xml">        <Person>          <Children>            <Person Name="Homer" Age="38" MainHobby="Complaining"/>            <Person Name="Bart" Age="10" MainHobby="Disturbing"/>            <Person Name="Marge" Age="10" MainHobby="Cooking"/>            <Person Name="Lisa" Age="17" MainHobby="Cooking"/>          </Children>        </Person>      </Var>      <Control t="ui.Document">        <Item t="data_items.Items" Items="%$Items%"/>        <Fields t="field.Fields">          <Field t="field.XmlMultipleGroup" Path="Children/Person">            <Presentation t="uiaspect.RichTable" />            <Field t="field.XmlField" Path="@Name"/>            <Field t="field.XmlField" Path="@Age"/>          </Field>          <Field t="field.CustomControl" Title="change and refresh">            <Control t="ui.ButtonOld" Text="Refresh Table" Class="change_text">              <OnClick t="action.RunActions">                <Action t="action.WriteValue" To="%$Items/Children/Person[1]/@Name%" Value="Mukki"/>                <Action t="field.RefreshField" FieldID="Children"/>              </OnClick>            </Control>          </Field>        </Fields>      </Control>      <RunOnControl t="uiaction.ButtonClick" RunOn=".change_text"/>      <ExpectedResult t="yesno.Contains">        <Text value="Mukki"/>      </ExpectedResult>    </Usage>  <Usage t="ui.ControlUsage" Of="field.RefreshField" IsTest="false">      <Var name="Item" t="xml.Xml">        <Person Name="Homer" Age=""/>      </Var>      <Control t="ui.Document">        <Item t="data_items.Items" Items="%$Item%"/>        <Fields t="field.Fields">          <Field t="field.XmlField" Path="@Name"/>          <Field t="field.XmlField" Path="@Age"/>        </Fields>        <Operations t="operation.Operation" ID="changeAge" Title="change age">          <Action t="operation.RunXtmlAction">            <Action t="action.RunActions">              <Action t="action.WriteValue" To="%@Age%" Value="30"/>              <Action t="field.RefreshField" FieldID="Age"/>            </Action>          </Action>        </Operations>        <Aspect t="uiaspect.Toolbar"/>      </Control>      <RunOnControl t="uiaction.ButtonClick" RunOn=".changeAge"/>      <ExpectedResult t="yesno.Contains">        <Text value="30"/>      </ExpectedResult>    </Usage>  <Usage t="ui.ControlUsage" Of="field_aspect.Calculated.ManualOverride">      <Var name="Products" t="xml.Xml">        <products>          <product name="car" price="100" discount="10" total=""/>        </products>      </Var>      <Control t="ui.Document">        <Item t="data_items.Items" Items="%$Products/*%"/>        <Fields t="field.Fields">          <Field t="field.XmlField" Path="@name">            <FieldAspect t="field_aspect.RefreshDependentFields" FieldsIds="total" />          </Field>          <Field t="field.XmlField" Path="@price">            <FieldAspect t="field_aspect.RefreshDependentFields" FieldsIds="total" RefreshScope="item"/>          </Field>          <Field t="field.XmlField" Path="@discount">            <FieldAspect t="field_aspect.RefreshDependentFields" FieldsIds="total" RefreshScope="container" RefreshOn="exit field"/>          </Field>          <Field t="field.XmlField" Path="@total">            <FieldAspect t="field_aspect.Calculated" ManualOverride="true">              <Formula t="data.Math" Expression="%@price% * (100 - %@discount%) / 100"/>            </FieldAspect>            <FieldAspect t="field_aspect.Description" Description="Click to change value"/>          </Field>        </Fields>      </Control>      <RunOnControl t="action.RunActions">        <Action t="uiaction.SetText" RunOn=".fld_price" Text="30"/>      </RunOnControl>      <ExpectedResult t="yesno.Contains">        <Text value="27"/>      </ExpectedResult>    </Usage>  <Usage t="ui.ControlUsage" Of="field.BooleanFilter">      <Var name="Items" t="xml.Xml">        <Persons>          <Person Name="Homer" Age="38" Male="true" MainHobby="Complaining"/>          <Person Name="Bart" Age="10" Male="true" MainHobby="Disturbing"/>          <Person Name="Marge" Age="10" Male="false" MainHobby="Cooking"/>          <Person Name="Lisa" Age="17" Male="false" MainHobby="Cooking"/>        </Persons>      </Var>      <Control t="ui.ItemList">        <Items t="data_items.Items" Items="%$Items/Person%"/>        <Fields t="field.Fields">          <Field t="field.XmlField" Path="@Name"/>          <Field t="field.XmlField" Path="@Age">            <Type t="field_aspect.Number"/>          </Field>          <Field t="field.XmlField" Path="@Male">            <Type t="field_aspect.Boolean"/>          </Field>        </Fields>        <Aspect t="uiaspect.Table"/>        <Aspect t="uiaspect.ExposedFiltersSection" FieldIds="Age,Name,Male"/>      </Control>      <RunOnControl t="picklist.Pick" FieldID="Male_filter" OptionIndex="1"/>      <ExpectedResult t="yesno.And">        <Item t="yesno.Contains" IgnoreCase="true">          <Text value="Bart"/>        </Item>        <Item t="yesno.NotContains" Text="Marge"/>      </ExpectedResult>    </Usage>  <Usage t="ui.ControlUsage" Of="field.TextFilter">      <Var name="Items" t="xml.Xml">        <Persons>          <Person Name="Homer" Age="38" MainHobby="Complaining"/>          <Person Name="Bart" Age="10" MainHobby="Disturbing"/>          <Person Name="Marge" Age="10" MainHobby="Cooking"/>          <Person Name="Lisa" Age="17" MainHobby="Cooking"/>        </Persons>      </Var>      <Control t="ui.ItemList">        <Items t="data_items.Items" Items="%$Items/Person%"/>        <Fields t="field.Fields">          <Field t="field.XmlField" Path="@Name"/>          <Field t="field.XmlField" Path="@Age">            <Type t="field_aspect.Number"/>          </Field>        </Fields>        <Aspect t="uiaspect.Table"/>        <Aspect t="uiaspect.ExposedFiltersSection" FieldIds="Age,Name"/>      </Control>      <RunOnControl t="action.RunActions">        <Action t="uiaction.SetText" RunOn=".fld_Name_filter" Text="bar"/>      </RunOnControl>      <ExpectedResult t="yesno.Contains">        <Text value="Bar"/>      </ExpectedResult>    </Usage><Usage t="ui.ControlUsage" Of="field_aspect.Picklist.BlinkingBug">  <Var name="Cities" t="picklist.DynamicOptions" OptionCode="%@code%" OptionLabel="%@title%">    <Options t="data.Pipeline">      <Item t="xml.Xml">        <cities>          <city code="1" title="TA"/>          <city code="2" title="NY"/>          <city code="3" title="London"/>        </cities>      </Item>      <Item value="%city%"/>    </Options>  </Var>  <Data t="xml.Xml">    <address city="1" number="3"/>  </Data>  <Control t="ui.Document">    <Item t="data_items.Items" Items="%%"/>    <Fields t="field.Fields">      <Field t="field.XmlField" Path="@city">        <FieldAspect t="field_aspect.Picklist" Options="%$Cities%"/>      </Field>      <Field t="field.XmlField" Path="@number">        <Type t="field_aspect.Text"/>      </Field>    </Fields>  </Control>  <ExpectedResult t="yesno.Contains">    <Text value="TA"/>  </ExpectedResult>  <RunOnControl t="picklist.Pick" FieldID="city"/></Usage>  <Usage t="ui.ControlUsage" Of="field_aspect.Picklist.Cities">      <Var name="Cities" t="picklist.DynamicOptions" OptionCode="%@code%" OptionLabel="%@title%">        <Options t="data.Pipeline">          <Item t="xml.Xml">            <cities>              <city code="1" title="TA"/>              <city code="2" title="NY"/>              <city code="3" title="London"/>            </cities>          </Item>          <Item value="%city%"/>        </Options>      </Var>      <Var name="Countries" value="Afghanistan,Albania,Algeria,Andorra,Angola,Antigua &amp; Deps,Argentina,Armenia,Australia,Austria,Azerbaij