<xtml ns="gstudio">  <Component id="OpenStudioCodeMirrorPopup" type="action.Action">    <Param name="PopupTitle" />    <Param name="CodeMirrorMode" type="enum" Options="javascript,xml,css,htmlmixed" Default="xml" />    <Param name="XmlToEdit" />    <Param name="IsValueInCData" type="data.Boolean" />    <Param name="OnUpdate" type="action.Action" script="true" />    <xtml t="popup.OpenPopup" PopupTitle="%$PopupTitle%">      <Var name="TempStringData" varScope="Component" t="data.IfThenElse" If="%$IsValueInCData%">        <Then t="xml.CDataValue" Element="%$XmlToEdit%" />        <Else t="xml.XmlToText" Data="%$XmlToEdit%" PrettyPrint="true" />      </Var>      <Var name="TempData" varScope="Component" t="data.WritableText" DefaultValue="%$TempStringData%" />      <Style t="gstudio.GoogleDrivePopup"/>            <Contents t="field.InnerPage" Title="CodeMirror">        <Field t="fld.Field" FieldData="%$TempData%" ID="gstutio_codemirror_in_popup" Title="Code Mirror">          <FieldType t="fld_type.EditableText">            <Style t="editable_text.CodeMirror" Readonly="false" Autofocus="false" LineNumbers="false" LineWrapping="false" EnableFullScreen="true" Mode="%$CodeMirrorMode%">              <Width t="ui.ScreenSize" Axis="width" Margin="250" AsHtmlString="true"/>              <Height t="ui.ScreenSize" Axis="height" Margin="200" AsHtmlString="true"/>            </Style>          </FieldType>        </Field>        <Field t="control.Layout" ID="Horizontal" Title="Buttons">          <Layout t="layout.HorizontalHtmlTable" Spacing="10px"/>          <Field t="control.Button" ID="gstudio_rename_OK" Title="Update" HideTitle="true">            <Style t="btn.JBart"/>            <Action t="popup.CloseContainingPopup" ExitMode="OK">              <DoOnExit t="action.RunActions">                <Action t="xml.ReplaceElement" Element="%$XmlToEdit%">                  <NewElement t="xml.TextToXml" Data="%$TempData%" />                </Action>                <Action t="xtml.UseParam" Param="OnUpdate" />              </DoOnExit>            </Action>          </Field>          <Field t="control.Button" ID="Cancel" Title="Close">            <Style t="btn.JBartGrey"/>            <Action t="popup.CloseContainingPopup"/>          </Field>  <FieldAspect t="field_feature.Css">    <Css><![CDATA[#this {  position:absolute;  right:0;  top:15px;}#wrapper {  position:relative;  height:35px;}]]></Css>  </FieldAspect>        </Field>      </Contents>    </xtml>  </Component>  <Component id="OpenStudioEditJSPopup" type="action.Action">    <Param name="Xtml" />    <Param name="ParamXml" />    <Param name="OnUpdate" type="action.Action" script="true" />    <xtml t="popup.OpenPopup" PopupTitle="Edit javascript">      <Style t="gstudio.GoogleDrivePopup"/>            <Contents t="field.InnerPage" Title="CodeMirror">        <Field t="field_dt.CodeMirrorTab" ParamXml="%$ParamXml%" Xtml="%$Xtml%" Large="" Type="js"/>      </Contents>    </xtml>  </Component>  <Component id="ShowPluginsPopup" type="action.Action">    <xtml t="popup.OpenPopup" PopupTitle="Widget Plugins">      <Style t="gstudio.GoogleDrivePopup" />      <Contents t="field.InnerPage">        <Field t="control.Layout">            <Field t="field.Layout" ID="DT_Widget_Properties_Basic" Title="Plugins">              <Field t="field.XtmlControl" ID="_XtmlControl" Title="XtmlControl">                <Control t="bart_dt.Plugins"/>              </Field>            </Field>      <Field t="control.Layout" ID="Horizontal" Title="Buttons">        <Field t="control.Button" ID="Close" Title="Close">          <Style t="btn.JBartGrey"/>          <Action t="popup.CloseContainingPopup"/>        </Field>        <FieldAspect t="field_feature.Css">          <Css><![CDATA[#this { margin-top: 23px; } ]]></Css>        </FieldAspect>      </Field>                      </Field>      </Contents>    </xtml>  </Component><Component id="EditXtmlComponentsDialog" type="action.Action">  <xtml t="popup.OpenPopup" PopupTitle="Xtml Components">    <Style t="gstudio.GoogleDrivePopup"/>    <Contents t="field.InnerPage" Title="Xtml Components">      <Field t="field.ItemListContainer" ID="_Cntr" Title="components">        <Field t="control.Layout" ID="_Cntr_Toolbar" Title="Toolbar">          <Layout t="layout.HorizontalCss3" Spacing="8px"/>          <FieldAspect t="itemlist_aspect.RefreshOnSelectionChange"/>          <Field t="control.Button" ID="Add__Cntr" Title="Add Component">            <Style t="btn.HyperlinkWithImage"/>            <Image t="image.Image" Url="images/add1616.gif" Width="" Height="">              <ImageProportions t="image.KeepProportions" FillImage="true" CenterImage="true"/>            </Image>            <Action t="popup.CloseContainingPopup">              <DoOnExit t="gstudio.AddANewComponent"/>            </Action>          </Field>          <Field t="control.Button" ID="Delete__Cntr" Title="Edit Xtml">            <Style t="btn.HyperlinkWithImage"/>            <Image t="gstudio.StudioSmallImage" PositionInSprite="224,0"/>            <FieldAspect t="itemlist_aspect.RefreshOnSelectionChange"/>            <Action t="xmlui.ShowXmlDialog" AutoWrite="true" Xml="%%" Title="xml of component %@id%" AutoWriteAllowTagChange=""/>          </Field>          <Field t="control.Button" ID="g_aaeditor" Title="AAEditor">            <Style t="btn.HyperlinkWithImage"/>            <Image t="gstudio.StudioSmallImage" PositionInSprite="240,0"/>            <FieldAspect t="itemlist_aspect.RefreshOnSelectionChange"/>            <Action t="operation.RunActions">              <Action t="popup.CloseContainingPopup" ExitMode=""/>              <Action t="gstudio_popup.ShowAAEditorFloatingPopup" Xtml="%xtml%"/>            </Action>          </Field>          <Field t="control.Button" ID="Delete__Cntr" Title="Delete Component">            <Style t="btn.HyperlinkWithImage"/>            <Image t="image.Image" Url="images/delete1616.gif" Width="" Height="">              <ImageProportions t="image.KeepProportions" FillImage="true" CenterImage="true"/>            </Image>            <FieldAspect t="itemlist_aspect.RefreshOnSelectionChange"/>            <Action t="popup_confirmation.OpenConfirmationPopup" Title="Deleting Component '%@id%'">              <Action t="itemlist_action.DeleteSelectedItem"/>              <Style t="popup_confirmation.GoogleDriveConfirmation"/>            </Action>          </Field>        </Field>        <Field t="field.Layout" ID="_Cntr_FilterGroup" Title="Filters">          <Layout t="layout.HorizontalCss3" Spacing="10"/>          <FieldAspect t="itemlist_aspect.FilterGroup"/>          <FieldAspect t="field_feature.Layout">            <Css><![CDATA[#this { padding-bottom: 5px; }]]></Css>          </FieldAspect>          <Field t="fld.Field" Title="Fast find" ID="Filter_fast_find" ReadOnlyText="" FieldData="%!@Filter_fastfind%">            <FieldType t="fld_type.EditableText">              <Style t="editable_text.JBartTextbox" Width="409px"/>            </FieldType>            <FieldAspect t="itemlist_aspect.Filter" FieldData="%@id%" ImmediateFilter="true">              <FilterType t="itemlist_filter.Text"/>            </FieldAspect>            <FieldAspect t="field_aspect.DescriptionForEmptyText" Description="Search"/>            <FieldAspect t="field_aspect.ClearValueButton"/>            <FieldAspect t="field_aspect.ItemListSelectionWithKeyboard"/>          </Field>        </Field>        <Items t="itemlist.XmlItems" ParentXml="%$_AppContext/AppXtml/../..%" Tag="Component"/>        <Field t="field.ItemList" Title="components" ID="components_table">          <View t="itemlist_style.JBartTable" MinWidth="410px"/>          <FieldAspect t="itemlist_aspect.ItemSelection"/>          <Field t="fld.Field" FieldData="%@id%" ID="id" Title="Component">            <FieldType t="fld_type.Text">              <Style t="uitext.PlainText"/>            </FieldType>          </Field>          <Field t="fld.Field" FieldData="%@type%" ID="type" Title="Type">            <FieldType t="fld_type.Text">              <Style t="uitext.PlainText"/>            </FieldType>          </Field>          <FieldAspect t="itemlist_aspect.DragAndDropItems">            <CssForDraggedElement><![CDATA[{ opacity: 0.9; }]]></CssForDraggedElement>            <CssForDraggedSpace><![CDATA[{ }]]></CssForDraggedSpace>          </FieldAspect>          <FieldAspect t="field_feature.Css">            <Css><![CDATA[#this {  max-height:400px;  overflow:auto;  }]]></Css>          </FieldAspect>        </Field>        <Layout t="layout.Vertical" Spacing="4px">        </Layout>      </Field>      <Field t="control.Layout" ID="Horizontal" Title="Buttons">        <Field t="control.Button" ID="Cancel" Title="Close">          <Style t="btn.JBartGrey"/>          <Action t="popup.CloseContainingPopup" ExitMode=""/>          <FieldAspect t="field_feature.Layout">            <Css><![CDATA[#this { margin-top: 15px; }#wrapper { }]]></Css>          </FieldAspect>        </Field>      </Field>      <Layout t="layout.Default"/>    </Contents>  </xtml></Component>  </xtml>