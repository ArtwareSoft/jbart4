<xtml ns="gstudio_select" light="false">  <Component id="OpenStudioSelectPopup" type="action.Action">      <Param name="PopupTitle" Default="Select"/>      <Param name="Items"/>      <Param name="ItemName" script="true" Default="%@name%"/>      <Param name="ItemDescription" script="true" Default="%@description%"/>      <Param name="DoOnSelect" type="action.Action" script="true"/>      <xtml t="popup.OpenPopup" PopupTitle="%$PopupTitle%">        <Style t="gstudio.GoogleDrivePopup"/>        <Contents t="field.InnerPage">          <Field t="field.ItemListContainer" ID="sgtudio_open_widgets" Title="widgets">            <Items t="itemlist.Items" Items="%$Items%"/>            <Field t="control.Layout" ID="Filters" Title="Filters">              <Layout t="layout.HorizontalHtmlTable" Spacing="0" VerticalAlign="top"/>              <FieldAspect t="itemlist_aspect.FilterGroup"/>              <FieldAspect t="field_feature.Css">                <Css><![CDATA[#this { padding-bottom: 8px; } #wrapper {}]]></Css>              </FieldAspect>              <Field t="fld.Field" Title="Search" ID="Search" FieldData="%@Search%">                <FieldType t="fld_type.EditableText">                  <Style t="ui.CustomCss" base="editable_text.JBartTextbox">                    <Style t="editable_text.JBartTextbox"/>                    <Css><![CDATA[#this { background:url(images/css/shadow2.png) repeat-x scroll 0 0 transparent;   font-size: 11px; width: 524px; height: 16px; padding-top: 7px; padding-right: 7px;   padding-bottom: 7px; padding-left: 7px; border: 1px solid #BDC7D8; }#this:focus {   box-shadow:  inset 0 1px 1px 0 rgba(0, 0, 0, 0.075),  0 0 8px 0 rgba(82, 168, 236, 0.6);   border-color: rgba(82, 168, 236, 0.8); outline: 0;   outline: thin dotted 9; }#this.placeholder { color:#8B8B8B; }#this.aa_error { color:#ec643d ; background:#fbebeb; border: 1px solid #feb2b2; }]]></Css>                  </Style>                </FieldType>                <FieldAspect t="field_aspect.DescriptionForEmptyText" Description="Search"/>                <FieldAspect t="itemlist_aspect.Filter">                  <FieldData t="xtml.UseParam" Param="ItemName"/>                  <FilterType t="itemlist_filter.Text"/>                </FieldAspect>                <FieldAspect t="field_aspect.ClearValueButton"/>                <FieldAspect t="field_aspect.ItemListSelectionWithKeyboard" />              </Field>            </Field>            <Field t="field.ItemList" Title="widgets" ID="sgtudio_open_widgets_inner">                      <FieldAspect t="itemlist_aspect.ItemClick">                        <OnClick t="action.RunActions">                    <Action t="popup.CloseContainingPopup"/>                    <Action t="xtml.UseParam" Param="DoOnSelect"/>                  </OnClick>                </FieldAspect>              <View t="ui.CustomCss" base="itemlist_style.JBartTable">                <Style t="itemlist_style.JBartTable">                </Style>                <Css><![CDATA[#this >table { color:#404040; padding-right: 20px; border: 1px #ccc solid;   min-width: 540px; border-collapse: collapse; border-spacing: 0px; }#this>table>tbody>tr.aa_details_inplace>td { background:#FAFADC; }#this>table>tbody>tr>td { color:#404040; padding-top: 5px; padding-right: 5px;   padding-bottom: 5px; padding-left: 5px;   border-bottom: 1px solid #ccc; border-left: 1px solid #ccc;   vertical-align:top; border-collapse: collapse;   border-spacing: 0px; }#this>table>thead>tr>th {   background:#eee;   padding-top: 4px; padding-right: 5px; padding-bottom: 4px;   padding-left: 5px; border-left: 1px solid #CCC;   border-bottom: 1px solid #CCC; }#this>table>thead>tr>th>.title { color:#666; font-weight:bold; font-size: 11px;   padding-right: 5px; }#this>table>thead>tr>th>.sortArrow { cursor:default; padding-right: 10px; }#this>table>thead>tr>th.sort_ascending >.sortArrow {   background:url(images/css/sort2.gif) no-repeat 0 5px;   }#this>table>thead>tr>th.sort_descending >.sortArrow {   background:url(images/css/sort1.gif) no-repeat 0 5px;   }]]></Css>              </View>              <FieldAspect t="itemlist_aspect.ShowTextWhenNoItems" Text="No items match the search"/>              <FieldAspect t="field_feature.Css">                <Css><![CDATA[#this {  height: 300px;  overflow: auto;}#wrapper {}]]></Css>              </FieldAspect>              <Field t="control.Label" ID="description" Title="Name">                <Text t="xtml.UseParam" Param="ItemName"/>                <Style t="uitext.PlainText"/>                <FieldAspect t="field_feature.Css">                  <Css><![CDATA[#this {  overflow: hidden;  cursor: pointer;  width: 250px;  max-height: 30px;}#wrapper {}]]></Css>                </FieldAspect>              </Field>              <FieldAspect t="itemlist_aspect.ItemSelection" KeyboardSupport="true" AlwaysOneSelected="true" MouseSupport="mouse hover">                <Css><![CDATA[{ background: #D9E8FB; }]]></Css>              </FieldAspect>              <Field t="control.Label" ID="description" Title="Description">                <Text t="xtml.UseParam" Param="ItemDescription"/>                <Style t="uitext.PlainText"/>                <FieldAspect t="field_feature.Css">                  <Css><![CDATA[#this {  overflow: hidden;  width: 250px;  max-height: 30px;}#wrapper {}]]></Css>                </FieldAspect>              </Field>            </Field>            <Layout t="layout.Default"/>          </Field>          <Field t="control.Layout" ID="Group" Title="Group">            <Layout t="layout.Vertical" Spacing="0"/>            <Field t="control.Button" ID="Close" Title="Close">              <Style t="btn.JBartGrey"/>              <FieldAspect t="field_feature.Css">                <Css><![CDATA[#this {  float:right;}#wrapper {}]]></Css>              </FieldAspect>              <Action t="popup.CloseContainingPopup" ExitMode=""/>            </Field>            <FieldAspect t="field_feature.Css">              <Css><![CDATA[#this { height: 32px; padding-top: 16px; }#wrapper { }]]></Css>            </FieldAspect>          </Field>        </Contents>      </xtml>    </Component></xtml>