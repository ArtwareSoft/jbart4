<xtml ns="gstudio"><Component id="MainToolbar" type="field.Fields" hidden="true" dtsupport="false">  <xtml t="google_drive.Toolbar" Title="toobar" ID="studio_main_toolbar">    <Field t="google_drive.ExpandCollapseToolbarButtons"/>    <Field t="control.Button" ID="GButton_Inspect" Title="Select">      <Style t="google_drive.StudioToolbarIcon"/>      <Action t="bart_dt.Inspect"/>      <Image t="gstudio.StudioSmallImage" PositionInSprite="0,0"/>      <FieldAspect t="field_feature.CssClass" ClassName="checked" ConditionForClass="%$_JBartStudio/Mode% == 'select'"/>    </Field>    <Field t="control.Button" ID="gstudio_toolbar_save" Title="Save">      <Style t="google_drive.StudioToolbarIcon"/>      <Action t="gstudio.Save" AlsoDeploy="%$_DomEvent/ctrlKey%"/>      <Image t="gstudio.StudioSmallImage" PositionInSprite="48,0"/>      <Disabled t="yesno.Expression" Expression="%$_JBartStudio/DisableSave%"/>    </Field>    <Field t="control.Button" ID="gstudio_toolbar_refresh" Title="Refresh">      <Style t="google_drive.StudioToolbarIcon"/>      <Action t="action.IfThenElse" If="%$_DomEvent/ctrlKey%">        <Then t="gstudio.StrongRefresh"/>        <Else t="gstudio.Refresh"/>      </Action>      <Image t="gstudio.StudioSmallImage" PositionInSprite="80,0"/>    </Field>    <Field t="control.Button" ID="gstudio_toolbar_layout" Title="Drag Mode">      <Style t="google_drive.StudioToolbarIcon"/>      <Action t="gstudio_drag.StartDragMode"/>      <Image t="gstudio.StudioSmallImage" PositionInSprite="112,0"/>      <FieldAspect t="field_feature.CssClass" ClassName="checked" ConditionForClass="%$_JBartStudio/Mode% == 'drag'"/>    </Field>    <Field t="control.Button" ID="gstudio_toolbar_layout" Title="Customize Style">      <Style t="google_drive.StudioToolbarIcon"/>      <Action t="gstudio.CustomizeStyle"/>      <Image t="gstudio.StudioSmallImage" PositionInSprite="144,0"/>    </Field>    <Field t="control.SeparatingLine" ID="SeparatingLine" Title="SeparatingLine">      <Style t="gstudio.StudioToolbarLine"/>    </Field>    <Field t="gstudio.AddFieldsInToolbar"/>    <Field t="control.SeparatingLine" ID="SeparatingLine" Title="SeparatingLine">      <Style t="gstudio.StudioToolbarLine"/>    </Field>    <Field t="control.Button" ID="gstudio_toolbar_refresh" Title="Edit Xtml">      <Style t="google_drive.StudioToolbarIcon"/>      <Action t="gstudio.EditScriptXtml"/>      <Image t="gstudio.StudioSmallImage" PositionInSprite="224,0"/>    </Field>    <Field t="control.Button" ID="gstudio_toolbar_aae" Title="Edit in AAEditor">      <Style t="google_drive.StudioToolbarIcon"/>      <Action t="gstudio.EditScriptXtmlInAAEditor"/>      <Image t="gstudio.StudioSmallImage" PositionInSprite="240,0"/>    </Field>    <Field t="control.Button" ID="gstudio_toolbar_refresh" Title="Delete">      <FieldAspect t="field_aspect.Title" Title="Delete Selected (%$_JBartStudio/SelectedFieldXtml/@Title%)"/>      <Action t="popup_confirmation.OpenConfirmationPopup" Title="Confirm Delete" Question="Are you sure you want to delete the field %@Title%?" LabelForYes="Delete" LabelForNo="Cancel">        <Data t="gstudio.SelectedFieldXtml"/>        <Style t="popup_confirmation.GoogleDriveConfirmation"/>        <Action t="action.RunActions">          <Var name="Parent" value="%..%"/>          <Action t="xml.Delete" Data="%%"/>          <Action t="action.RunActions">            <Action t="gstudio.Refresh"/>            <Action t="bart_dt.EditXtmlInStudioBar" Data="%$Parent%"/>          </Action>        </Action>      </Action>      <Style t="google_drive.StudioToolbarIcon"/>      <Image t="gstudio.StudioSmallImage" PositionInSprite="256,0"/>    </Field>    <Field t="control.SeparatingLine" ID="SeparatingLine" Title="SeparatingLine">      <Style t="gstudio.StudioToolbarLine"/>    </Field>    <Field t="control.Button" ID="gstudio_toolbar_properties" Title="Show properties window">      <Style t="google_drive.StudioToolbarIcon"/>      <Action t="gstudio_popup.ShowProperties"/>      <Image t="gstudio.StudioSmallImage" PositionInSprite="192,0"/>      <FieldAspect t="field_feature.CssClass" ClassName="checked">        <ConditionForClass t="popup.IsFloatingPopupOpen" Identifier="properties"/>      </FieldAspect>    </Field>    <Field t="control.Button" ID="gstudio_toolbar_outline" Title="Show outline">      <Style t="google_drive.StudioToolbarIcon"/>      <Action t="gstudio_popup.ShowOutline"/>      <Image t="gstudio.StudioSmallImage" PositionInSprite="208,0"/>      <FieldAspect t="field_feature.CssClass" ClassName="checked">        <ConditionForClass t="popup.IsFloatingPopupOpen" Identifier="outline"/>      </FieldAspect>    </Field>    <Field t="control.Button" ID="gstudio_toolbar_instance_settings" Title="Show instance settings">      <Condition t="yesno.OR">        <Item t="yesno.NotEmpty">          <Value t="xtml.ComponentDefinition" ID="sample.settings"/>        </Item>        <Item t="yesno.NotEmpty">          <Value t="xtml.ComponentDefinition" ID="sample.Settings"/>        </Item>      </Condition>      <Style t="google_drive.StudioToolbarIcon"/>      <Action t="gstudio_popup.ShowSettingsPage">        <SettingsPage t="data.IfThenElse">          <If t="yesno.NotEmpty">            <Value t="xtml.ComponentDefinition" ID="sample.settings"/>          </If>          <Then t="sample.settings"/>          <Else t="sample.Settings"/>        </SettingsPage>      </Action>      <Image t="gstudio.StudioSmallImage" PositionInSprite="288,0"/>      <FieldAspect t="field_feature.CssClass" ClassName="checked">        <ConditionForClass t="popup.IsFloatingPopupOpen" Identifier="instance settings"/>      </FieldAspect>    </Field>    <Field t="instance_editor.StudioToolbarExtension" Condition="%$_WidgetXml/@jbartapp%"/>    <Field t="gstudio.ErrorsButtonInToolbar"/>  </xtml></Component>  <Component id="ErrorsButtonInToolbar" type="field.Fields" light="false">    <xtml t="control.Button" ID="toolbar_errors_sign" Title="Errors">      <Style t="google_drive.StudioToolbarIcon"/>      <Image t="gstudio.StudioSmallImage" PositionInSprite="64,0"/>      <Action t="debugui.ShowDebugUi"/>      <FieldAspect t="field_aspect.Css" Class="hidden"/>      <FieldAspect t="field_feature.Css">        <Css><![CDATA[#this.hidden { display:none;}]]></Css>      </FieldAspect>    </xtml>  </Component>  <Component id="ShowSelected" type="action.Action">    <xtml t="bart_dt.ShowSelectedObjectElements" />  </Component></xtml>