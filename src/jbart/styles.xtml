  border-width: 8px 0px 8px 8px;
  border-left-color: #999;
  margin: 33px 0 0 9px;
}]]>
    </Css>
    <Javascript>
<![CDATA[function(itemlist) { 
  itemlist.sliderWidth = parseInt('%$Width%'.match(/[0-9]+/)[0]);
  jQuery(itemlist).find('>.next,>.prev').each(function() { ajaxart_disableSelection(this) });
                                                                   
  itemlist.bind('widthChanged',function(eventProps) {
    itemlist.sliderWidth = eventProps.width - 100; 
    jQuery(itemlist).find('>div>.aa_slider_top').width(itemlist.sliderWidth);
  });
  
  itemlist.setItems({
    itemElement: '.aa_item',
    initItem: function(item) {     
      item.setFields();
    }
  });
  jQuery(itemlist).find('>.aa_slider_top').width(itemlist.sliderWidth);
  var body = jQuery(itemlist).find('>div>.aa_slider_body');
  var currLeft = 0;
  body.css('left','0');
  jQuery(itemlist).find('>.prev').click(function() {
    if (currLeft > - itemlist.sliderWidth) {
      body.animate( { left: '0'} ,'slow');
      currLeft = 0;
      return;
    } else {
      body.animate( { left: '+='+itemlist.sliderWidth },'slow');
      currLeft += itemlist.sliderWidth;
    }
  });
  jQuery(itemlist).find('>.next').click(function() {
    var lastItem= jQuery(itemlist).find('>div>.aa_slider_body>div:last');
    var lastLeft = lastItem[0].offsetLeft + currLeft;
    if (lastLeft + lastItem.outerWidth(true) <= itemlist.sliderWidth ) return;
    
    body.animate( { left: '-='+itemlist.sliderWidth },'slow');
    currLeft -= itemlist.sliderWidth;
  });
  
  itemlist.bind('clearItems',function() {
     currLeft = 0;
     body.css('left','0');
  });
}                      
]]>
    </Javascript>
  </xtml>
