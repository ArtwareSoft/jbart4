e="Property Sheet">                <Style t="properties.LMCDialogPropertySheet"/>                <Field t="fld.Field" FieldData="%%" ID="newname" Title="Enter a new name">                  <FieldType t="fld_type.EditableText">                    <Style t="editable_text.LMCTextbox" Width="273px">&#xa;                    </Style>                  </FieldType>                  <FieldAspect t="field_aspect.Mandatory" ErrorMessage="Please provide a widget name"/>                  <FieldAspect t="field_feature.HandleEvent">                    <Event t="field_feature.EnterPressed"/>                    <Action t="operation.ClickOnButton" Button="gstudio_rename_OK"/>                  </FieldAspect>                  <FieldAspect t="field_aspect.DefaultValue">                    <Value t="text.FirstSucceeding">                      <Item value="%$Project/@name%"/>                      <Item value="%$ProjectSettings/@name%"/>                    </Value>                  </FieldAspect>                </Field>              </Field>              <Field t="control.Layout" ID="Horizontal" Title="Buttons">                <Layout t="layout.DialogButtonsRightAlign"/>                <Field t="control.Button" ID="gstudio_rename_OK" Title="OK" HideTitle="true">                  <Style t="btn.LMCButton" Width="121px"/>                  <Action t="popup.CloseContainingPopup" ExitMode="OK">                    <DoOnExit t="action.RunActions">                      <Action t="action.WriteValue" Value="%%" To="%$Project/!@name%"/>                      <Action t="action.WriteValue" To="%$ProjectSettings/!@name%" Value="%%"/>                      <Action t="field.RefreshField" FieldID="lmc_top_name" FireOnUpdate="" Scope="screen"/>                      <Action t="lmc.SaveLMCProject"/>                      <Action t="lmc.SaveVisitorHtmlForInstance"/>                    </DoOnExit>                  </Action>                </Field>                <Field t="control.Button" ID="Cancel" Title="Cancel">                  <Style t="btn.LMCGreyButton" Width="140px"/>                  <Action t="popup.CloseContainingPopup" ExitMode=""/>                </Field>                <FieldAspect t="field_feature.Css">                  <Css><![CDATA[#this { margin-top: 23px; }]]></Css>                </FieldAspect>              </Field>              <FieldAspect t="field_aspect.FieldData">                <FieldData t="data.WritableText" DefaultValue="%$Project/@name%"/>              </FieldAspect>            </Field>          </Contents>          <Style t="popup.LMCDialog"/>        </Action>        <ButtonText t="text.FirstSucceeding">          <Item value="%$Project/@name%"/>          <Item value="%$ProjectSettings/@name%"/>        </ButtonText>      </Field>      <Field t="control.Hyperlink" ID="best_run_on_chrome" Title="Best run agent on chrome browser" Target="_blank" Link="https://www.google.com/intl/en/chrome/browser/">        <Style t="ui.CustomCss" base="hyperlink_style.Simple">          <Style t="hyperlink_style.Simple"/>          <Css><![CDATA[#this { color: #f21d41; font-weight: bold;   font-size: 16px; top: 9px; left: 36\%;   position: absolute; text-align: center;   text-decoration: none; }#this:hover { text-decoration: underline; }]]></Css>        </Style>        <FieldAspect t="field_aspect.Hidden">          <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render">            <ShowCondition t="yesno.Not">              <Of t="ui.IsChrome"/>            </ShowCondition>          </OnCondition>        </FieldAspect>      </Field>      <Field t="control.Button" ID="_Button" Title="Menu">        <Style t="ui.CustomCss" base="btn.MobilePagesButton">          <Style t="btn.MobilePagesButton"/>          <Css><![CDATA[#this {  position:absolute;  top:12px;  right:12px;  cursor:pointer;}.bline {  border-right:1px solid #777C80;  height:3px;  background:#fff;  margin-bottom:3px;  width:20px;}]]></Css>        </Style>        <FieldAspect t="popup_field_aspect.ToolbarPopup" ToolbarID="lmc_top_toolbar">          <PopupStyle t="ui.CustomCss" base="google_drive.StudioMenuStyle">            <Style t="google_drive.StudioMenuStyle"/>            <Css><![CDATA[#this>.aa_popup_frame {  background:#fff;  min-width:280px;  min-height:30px;  box-shadow:2px 2px 3px #d5d5d5;  padding:3px 0;  border:1px solid rgb(213, 213, 213);}#this>.aa_popup_frame>.aa_popup_contents {} #this>.aa_popup_frame:focus {  outline:none;}.aa_popup_remove_top_border {  display: none;}]]></Css>          </PopupStyle>          <Contents t="field.InnerPage" Title="Menu">            <Layout t="layout.Default"/>            <Field t="control.Button" ID="_Button2106" Title="Preview room" HideTitle="true">              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="operation.Link" OpenInANewTab="true" Link="http://jb-letmesee.appspot.com/LetMeSee/%$Project/@id%.html?roomid=%$Room/@id%&amp;notracking=true">&#xa;              </Action>              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$Room/@id% != ''">&#xa;                </OnCondition>              </FieldAspect>              <ButtonText t="data.IfThenElse" If="%$Room/@isTemplate%" Else="Preview customer room in desktop" Then="Preview template in desktop"/>            </Field>            <Field t="control.Button" ID="_Button2108" Title="Edit room style (simple)" HideTitle="true">              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="action.RunActions">                <Action t="popup.CloseContainingPopup" ExitMode=""/>                <Action t="xml.RegisterModifyOnXml" SingletonId="RoomStyle" Xml="%$Room/!style%">                  <Action t="lmc.RefreshLMCPreview" ChangedXml="%%" DelayInMiliseconds="300"/>                </Action>                <Action t="popup.OpenPopup" PopupTitle="Style Editor">                  <Style t="popup.LMCDialog" AutoFocus="false" Opacity="none">&#xa;                  </Style>                  <Contents t="field.InnerPage">                    <Layout t="layout.Default"/>                    <Field t="control.Layout" ID="_Vertical_Layout" Title="Vertical Layout">                      <Layout t="layout.Default">&#xa;                      </Layout>                      <Field t="control.Page" ID="_Link_to_page5" Title="Link to page">                        <Page t="data.IfThenElse" If="%$_DomEvent/ctrlKey%">                          <Then t="lmc.StyleEditor"/>                          <Else t="lmc.LightStyleEditor"/>                        </Page>                      </Field>                    </Field>                  </Contents>                  <Feature t="popup.UniquePopup" UniqueID="lmc_edit_style_simple"/>                </Action>              </Action>              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$Room/@id% != ''">&#xa;                </OnCondition>              </FieldAspect>              <ButtonText t="data.IfThenElse" If="%$Room/@isTemplate%" Then="Edit template style..." Else="Edit room style..."/>            </Field>            <Field t="control.Button" ID="_Button" Title="Move customer room to archive">              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="popup_confirmation.OpenConfirmationPopup" Title="Archive Room" Question="Are you sure?">                <Style t="popup_confirmation.GoogleDriveConfirmation"/>                <Action t="async.RunAsyncActions" StopOnError="true">                  <ProgressStyle t="async_ui.ChangeCursorForProgress"/>                  <Action t="lmc.LMCMoveRoomToArchive" Room="%$Room%"/>                  <Action t="operation.ChangeTab" TabControl="Agent_room_tab" MoveTo="specific tab" TabToMoveTo="Customer_archive"/>                </Action>              </Action>              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$Room/@isTemplate% != 'true' &amp;&amp; %$Room/@id% != ''"/>              </FieldAspect>            </Field>            <Field t="control.Button" ID="_Button" Title="Delete customer room">              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="popup_confirmation.OpenConfirmationPopup" Title="Delete Room - %$Room/@customerName%">                <Style t="popup_confirmation.GoogleDriveConfirmation"/>                <Action t="async.RunAsyncActions" StopOnError="true">                  <Action t="lmc.DeleteLMCRoom" Room="%$Room%"/>                  <ProgressStyle t="async_ui.ChangeCursorForProgress"/>                </Action>              </Action>              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$Room/@isTemplate% != 'true' &amp;&amp; %$Room/@id% != ''"/>              </FieldAspect>            </Field>            <Field t="control.Button" ID="_Button" Title="Delete template">              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="popup_confirmation.OpenConfirmationPopup" Title="Delete Template - %$Room/@name%">                <Style t="popup_confirmation.GoogleDriveConfirmation"/>                <Action t="lmc.DeleteLMCRoom" Room="%$Room%"/>              </Action>              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$Room/@isTemplate% == 'true'"/>              </FieldAspect>            </Field>            <Field t="control.Button" ID="_Button21" Title="Duplicate customer room" HideTitle="true">              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$Room/@isTemplate% != 'true' &amp;&amp; %$Room/@id% != ''"/>              </FieldAspect>              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="lmc.OpenDuplicateRoomPopup" OriginalRoom="%$Room%">&#xa;              </Action>            </Field>            <Field t="control.Button" ID="_Button210" Title="Save customer room as template..." HideTitle="true">              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$Room/@isTemplate% != 'true'"/>              </FieldAspect>              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="popup.OpenPopup" PopupTitle="Save customer room as template">                <Style t="popup.LMCDialog"/>                <Contents t="field.InnerPage" Title="Create a template from customer room">                  <Layout t="layout.Default"/>                  <Field t="control.Layout" ID="NewRoom" Title="Vertical Layout">                    <Layout t="layout.Vertical" Spacing="30px"/>                    <Field t="control.PropertySheet" ID="_Property_Sheet1" Title="Property Sheet">                      <Style t="properties.LMCDialogPropertySheet"/>                      <Field t="fld.Field" FieldData="%!@name%" ID="name" Title="Template Name">                        <FieldType t="fld_type.EditableText">                          <Style t="editable_text.LMCTextbox" Width="272px"/>                        </FieldType>                        <FieldAspect t="field_aspect.Mandatory"/>                        <FieldAspect t="field_feature.HandleEvent">                          <Event t="field_feature.Update"/>                          <Action t="action.WriteValue" To="%!../@name%" Value="%%"/>                        </FieldAspect>                      </Field>                    </Field>                    <FieldAspect t="field_aspect.FieldData">                      <FieldData t="xml.WithChanges" Xml="%$Room%">                        <Change t="xml.SetAttribute" AttributeName="id" RemoveEmptyAttribute="true"/>                        <Change t="xml.WriteValue" To="%!@name%" Value="Template from %$Room/@customerName%"/>                        <Change t="xml.WriteValue" To="%!@isTemplate%" Value="true"/>                        <Change t="xml.WriteValue" To="%@customerName%"/>                        <Change t="xml.Delete" Data="%sms%"/>                      </FieldData>                    </FieldAspect>                    <Field t="control.Layout" ID="_Horizontal_Layout" Title="Buttons">                      <Layout t="layout.DialogButtonsRightAlign"/>                      <Field t="control.Button" ID="Cancel_Button" Title="Cancel" HideTitle="true">                        <Style t="btn.LMCGreyButton" Width="130px"/>                        <Action t="popup.CloseContainingPopup" ExitMode=""/>                      </Field>                      <Field t="control.Button" ID="OK" Title="Create" HideTitle="true">                        <Style t="btn.LMCButton" Width="130px" WaitingText="Creating..."/>                        <Action t="action.Validate" Group="NewRoom">                          <WhenValid t="async.RunAsyncActions" StopOnError="true">                            <Action t="lmc.CreateRoom" IsTemplate="" RoomXml="%%">                              <DoOnNewRoom t="lmc.SetCurrentRoom" RoomID="%@id%"/>                            </Action>                            <Action t="popup.CloseContainingPopup" ExitMode="OK"/>                          </WhenValid>                        </Action>                      </Field>                    </Field>                  </Field>                </Contents>              </Action>            </Field>            <Field t="control.Button" ID="_Button210" Title="Room as xml..." HideTitle="true">              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="lmc.OpenRoomXmlPopup"/>              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$Room/@isTemplate% != 'true' &amp;&amp; %$Room/@id% != ''">&#xa;                </OnCondition>              </FieldAspect>            </Field>            <Field t="control.Button" ID="_Button2101" Title="Template as xml..." HideTitle="true">              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="lmc.OpenRoomXmlPopup"/>              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$Room/@isTemplate% == 'true'"/>              </FieldAspect>            </Field>            <Field t="control.Button" ID="_Button2102" Title="Share template" HideTitle="true">              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="popup.OpenPopup" PopupTitle="Share Template">                <Style t="popup.LMCDialog"/>                <Contents t="field.InnerPage">                  <Layout t="layout.Default"/>                  <Field t="control.Label" ID="_Label" Title="Label" Text="Copy this link to the settings of the project you want to share this template with:">                    <Style t="ui.CustomCss" base="uitext.PlainText">                      <Style t="uitext.PlainText"/>                      <Css><![CDATA[#this { font-size: 14px; margin-bottom: 12px; }]]></Css>                    </Style>                  </Field>                  <Field t="fld.Field" ID="temp" Title="Temp">                    <FieldType t="fld_type.EditableText">                      <Style t="editable_text.LMCTextbox" Width="495px"/>                    </FieldType>                    <FieldData t="data.WritableText" DefaultValue="https://storage.googleapis.com/letmesee1/%$Project/@id%/rooms/%$Room/@id%.xml"/>                  </Field>                  <Field t="control.Layout" ID="popup_buttons" Title="Buttons">                    <Layout t="layout.DialogButtonsRightAlign"/>                    <Field t="control.Button" ID="OK" Title="Close" HideTitle="true">                      <Style t="btn.LMCButton" Width="140px"/>                      <Action t="popup.CloseContainingPopup" ExitMode="OK">                        <DoOnExit t="action.RunActions"/>                      </Action>                    </Field>                    <FieldAspect t="field_feature.Layout">                      <Css><![CDATA[#this { margin-top: 20px; }]]></Css>                    </FieldAspect>                  </Field>                </Contents>              </Action>              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$Room/@isTemplate% == 'true'"/>              </FieldAspect>            </Field>            <Field t="control.Button" ID="_Button2105" Title="Create multiple customer rooms..." HideTitle="true">              <Style t="google_drive.StudioMainMenuButton"/>              <Action t="popup.OpenPopup" PopupTitle="Create multiple customer rooms">                <Style t="popup.LMCDialog" Opacity=""/>                <Contents t="field.InnerPage" Title="Rooms">                  <Layout t="layout.Default"/>                  <Field t="control.Layout" ID="_Vertical_Layout" Title="Group">                    <Layout t="layout.Horizontal" Spacing="40"/>                    <FieldAspect t="field_aspect.FieldData" FieldData="%$Temp/!createRooms%"/>                    <Field t="field.ItemListContainer" ID="customers_cntr" Title="customers">                      <Items t="itemlist.XmlItems" ParentXml="%%" Tag="customer"/>                      <Field t="field.ItemList" Title="customers" ID="customers_table">                        <View t="ui.CustomCss" base="itemlist_style.JBartTable">                          <Style t="itemlist_style.JBartTable" MinWidth="540px"/>                          <Css><![CDATA[#this >table {  color:#404040;  min-width:540px;  border:1px #ccc solid;  border-collapse:collapse;  border-spacing:0px;  border:1px solid #CCC;}#this>table>tbody>tr.aa_details_inplace>td {  background:#FAFADC;}#this>table>tbody>tr>td {  vertical-align:top;  border-bottom:1px solid #ccc;  border-top:0px;  border-left:1px solid #ccc;  border-right:0px;  padding:2px;  color:#404040;  border-collapse:collapse;  border-spacing:0px;}#this>table>thead>tr>th {  border-left:1px solid #CCC;  border-bottom:1px solid #CCC;  background:#eee;  padding:4px 5px;}#this>table>thead>tr>th>.aa_table_title {  padding-right:5px;  font-weight:bold;  font-size:11px;  color:#666;}#this>table>thead>tr>th>.sortArrow {  padding-right:10px;  cursor:default;}#this>table>thead>tr>th.sort_ascending >.sortArrow {  background:url(_jbartImages_/css/sort2.gif) no-repeat 0 5px;}#this>table>thead>tr>th.sort_descending >.sortArrow {  background:url(_jbartImages_/css/sort1.gif) no-repeat 0 5px;}]]></Css>                        </View>                        <FieldAspect t="itemlist_aspect.ItemSelection"/>                        <FieldAspect t="itemlist_aspect.TableColumnDragAndDrop"/>                        <FieldAspect t="itemlist_aspect.DragAndDropItems"/>                        <FieldAspect t="itemlist_aspect.ShowTextWhenNoItems"/>                        <FieldAspect t="itemlist_aspect.UpdateOnAddOrDeleteItem"/>                        <Field t="fld.Field" FieldData="%!@name%" ID="name" Title="Customer Name">                          <FieldType t="fld_type.EditableText">                            <Style t="editable_text.LMCTextboxInTable"/>                          </FieldType>                          <FieldAspect t="field_aspect.Mandatory"/>                        </Field>                        <Field t="fld.Field" FieldData="%!@customerCompany%" ID="name1" Title="Customer Company">                          <FieldType t="fld_type.EditableText">                            <Style t="editable_text.LMCTextboxInTable"/>                          </FieldType>                          <FieldAspect t="field_aspect.Mandatory"/>                        </Field>                        <Field t="fld.Field" FieldData="%!@phone%" ID="phone" Title="Phone Number">                          <FieldType t="fld_type.EditableText">                            <Style t="editable_text.LMCTextboxInTable"/>                          </FieldType>                          <FieldAspect t="field_aspect.Mandatory"/>                        </Field>                      </Field>                      <Field t="control.Layout" ID="customers_cntr_Toolbar" Title="Toolbar">                        <Layout t="layout.Horizontal" Spacing="5px"/>                        <FieldAspect t="itemlist_aspect.RefreshOnSelectionChange"/>                        <FieldAspect t="field_feature.Layout">                          <Css><![CDATA[#this { margin-bottom: 8px; margin-top: 8px; }]]></Css>                        </FieldAspect>                        <Field t="control.Button" ID="customers_cntr_Add" Title="Add customer">                          <Style t="btn.HyperlinkWithImage"/>                          <Image t="img.Image" Url="_jbartImages_/add1616.gif" OriginalWidth="16" OriginalHeight="16">                            <AdjustSize t="img.Center" RightToLeft=""/>                          </Image>                          <Action t="itemlist_action.AddItemToItemList">                            <Item t="xml.ByTag" Tag="customer"/>                            <Location t="itemlist_action.AsLastItem"/>                          </Action>                        </Field>                        <Field t="control.Button" ID="customers_cntr_Delete" Title="Remove customer">                          <Style t="btn.HyperlinkWithImage"/>                          <Image t="img.Image" Url="_jbartImages_/delete1616.gif" OriginalWidth="16" OriginalHeight="16">                            <AdjustSize t="img.Center" RightToLeft=""/>                          </Image>                          <Action t="itemlist_action.DeleteSelectedItem"/>                        </Field>                        <Field t="control.Button" ID="properties_cntr_Add1" Title="Import / Export" HideTitle="true">                          <Image t="img.Image" Url="_jbartImages_/groupby.gif" OriginalWidth="16" OriginalHeight="16">                            <AdjustSize t="img.Center" RightToLeft=""/>                          </Image>                          <Style t="btn.HyperlinkWithImage"/>                          <Action t="popup.OpenPopup" PopupTitle="Import customers">                            <Style t="popup.LMCDialog" LightOpacity=""/>                            <Contents t="field.InnerPage">                              <Layout t="layout.Default"/>                              <Field t="control.Layout" ID="_Group" Title="Group">                                <Field t="control.Label" ID="_Label" Title="Label" Text="Paste text here:">                                  <Style t="uitext.PlainText"/>                                </Field>                                <Field t="fld.Field" FieldData="%$Import/!@text%" ID="text" Title="Text">                  