[CDATA[<div>  <div class="lmc_img" />  <div class="notify_text" /></div>]]></Html>        <Css><![CDATA[#this {  display:none;  left:0;  right:0;  z-index: 100;  background-color: #efe6f2;  opacity: 0.95;  border-top:3px solid #47BAC1;  height:65px;  }.notify_text {  position:absolute;  left: 50px;  top: 12px;  right:0;  text-align:center;    font-weight: bold;  font-size:16px;  color: #333;}#this.shown {  display: inline-block;}#this.aanotif_status_error {  background: pink;}.lmc_img {  position:absolute;  background:url('_jbartImages_/LetMeSee/40x40.png');  width:40px;  height:40px;  top:12px;  left:13px;}]]></Css>        <Javascript><![CDATA[function(notificationBox) {  aa_notification_box(notificationBox,{});  aa_positionFixed(notificationBox.el,{    bottom: 0,    object: notificationBox  });}function aa_notification_box(notificationBox,settings) {  var jBox = notificationBox.$el;  notificationBox.bind('notification',function(evt) {     notificationBox.setStatusClass( evt.status );     jBox.find('.notify_text').html(evt.text);     jBox.addClass('shown');     if (evt.status == 'clear') jBox.removeClass('shown');     aa_positionFixed(notificationBox.el,{        bottom: 0,        object: notificationBox     });     var height = jBox.height();     jBox.css('bottom','-'+height+'px')         jBox.css('opacity',1).animate({ bottom: '0'},1000);          setTimeout(function() {       jBox.animate({bottom: '-'+height+'px'},1000, function() { jBox.removeClass('shown');});      },4000);  });          }]]></Javascript>      </Style>    </Field>    <FieldAspect t="field_feature.Css">      <Css><![CDATA[#this {  padding-bottom:20px;}]]></Css>    </FieldAspect>  </xtml></Component><Component id="LMCShowForPage" type="field.FieldAspect">  <Param name="Page" />  <xtml t="field_aspect.Hidden">    <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do n