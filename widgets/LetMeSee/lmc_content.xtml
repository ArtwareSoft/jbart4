  <Field t="control.Layout" ID="_Group" Title="Buttons">              <Field t="control.Button" ID="_Button1" Title="Close">                <Action t="popup.CloseContainingPopup" ExitMode=""/>                <FieldAspect t="field_feature.Css">                  <Css><![CDATA[#this {}#wrapper {  padding-top:10px;}]]></Css>                </FieldAspect>                <Style t="btn.LMCGreyButton" Width="106px"/>              </Field>              <Layout t="layout.DialogButtonsRightAlign"/>              <Field t="notification_box.NotificationBox" ID="AddFromTemplateNotification" Title="NotificationBox" TextToShowOnPreview="">                <Style t="notification_box.Default"/>              </Field>            </Field>            <Field t="control.Label" ID="LoadingTemplateIndication" Title="Loading ...">              <Style t="ui.CustomStyle" base="uitext.PlainText">                <Html><![CDATA[<div />]]></Html>                <Css><![CDATA[#this {  top:14px;  left:397px;  position:absolute;}]]></Css>                <Javascript><![CDATA[function(textObj) {   aa_textLabel(textObj);  textObj.$el.hide();}]]></Javascript>              </Style>            </Field>          </Contents>          <Feature t="popup.PopupSize" Width="730px" Height=""/>          <Feature t="popup.StudioPopupFeature"/>        </Action>        <Disabled t="yesno.ConditionByXtml">          <Expression t="yesno.And">            <Item t="yesno.GreaterThan" Item="2">              <Than t="data.Pipeline">                <Item value="%$RoomHeaders/*%"/>                <Aggregator t="data.Count"/>              </Than>            </Item>            <Item t="yesno.IsEmpty">              <Value t="lmc.ExternalTemplates"/>            </Item>          </Expression>        </Disabled>      </Field>      <FieldAspect t="field_feature.Layout">        <Css><![CDATA[#this { margin-top: 10px; margin-left: 0; }#wrapper { }]]></Css>      </FieldAspect>    </Field>    <FieldAspect t="field_feature.HandleEvent">      <Event t="field_feature.Load"/>      <Action t="operation.RunXtmlAction">        <Action t="xml.RegisterModifyOnXml" SingletonId="RoomLiveData" Xml="%$Room/!items%">          <Action t="lmc.RefreshLMCPreview" ChangedXml="%%"/>        </Action>      </Action>    </FieldAspect>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="right_to_left" ConditionForClass="%$Room/style/general/@rtl%"/>  </xtml></Component>    <Component id="HtmlCleansing" type="jbart.MyWidgetPage">      <xtml t="control.Layout" ID="HtmlCleansing" Title="HtmlCleansing">        <Layout t="layout.HorizontalCss3" Spacing="50"/>        <Field t="control.Layout" ID="_Group2" Title="Settings">          <Field t="fld.Field" FieldData="%!$Temp/@cleansing%" ID="cleansing" Title="Cleansing">            <FieldType t="fld_type.MultiplePicklist">              <Options t="editable_picklist.OptionsByCommas" Options="plain text,images only,images and text,remove html table,adaptable width"/>              <Style t="multiple_picklist.Checkboxes"/>            </FieldType>            <FieldAspect t="field_feature.HandleEvent">              <Event t="field_feature.Update"/>              <Action t="action.RunActions">                <Action t="xml.ChangeXml" Xml="%$CleanedItem%">                  <Change t="xml.DeleteChildren"/>                  <Change t="xml.AddChildren" CloneChildren="false">                    <Children t="lmc.HtmlCleansing" Options="%%" Html="%$Temp/@source%"/>                  </Change>                </Action>                <Action t="action.WriteValue" To="%$Temp/@clean%">                  <Value t="data.Pipeline">                    <Item t="xml.XmlToText" Data="%$CleanedItem%"/>                  </Value>                </Action>                <Action t="field.RefreshField" FireOnUpdate="" FieldID="CleansingPreview,clean" Scope="screen"/>              </Action>            </FieldAspect>          </Field>          <Field t="control.PropertySheet" ID="_Property_Sheet" Title="Property Sheet">            <Style t="properties.TitlesAboveFields" HorizontalSpacing="15px" VerticalSpacing="8px"/>            <Field t="fld.Field" FieldData="%$Temp/!@source%" ID="source" Title="Source">              <FieldType t="fld_type.EditableText">                <Style t="editable_text.JBartMultiLine" Width="400px" Height="600px"/>              </FieldType>              <FieldAspect t="field_feature.HandleEvent">                <Event t="field_feature.Update"/>                <Action t="action.WriteValue" To="%!"/>              </FieldAspect>            </Field>            <Field t="fld.Field" FieldData="%$Temp/!@clean%" ID="clean" Title="Clean">              <FieldType t="fld_type.EditableText">                <Style t="editable_text.JBartMultiLine" Width="400px" Height="600px"/>              </FieldType>            </Field>          </Field>          <Layout t="layout.Default"/>          <FieldAspect t="field_aspect.FieldData" FieldData="%$Temp%"/>        </Field>        <Field t="control.Layout" ID="CleansingPreview" Title="Preview">          <FieldAspect t="field_aspect.VisualContainer" ID="cobrowse_main_cntr">            <Style t="ui.CustomCss" base="visual_container.MobileSkin">              <Style t="visual_container.MobileSkin"/>              <Css><![CDATA[#this { display: inline-block; position: relative;   border: 1px solid #c9c9c9;   -moz-border-radius: 11px;   -webkit-border-radius: 11px;   border-radius: 11px;   -moz-background-clip: padding;   -webkit-background-clip: padding-box;   background-clip: padding-box;   background-color: #45bbba;   -moz-box-shadow: 0 0 15px rgba(0,0,0,.28), inset 0 1px 1px rgba(255,255,255,.45), inset 0 0 2px rgba(255,255,255,.2);   -webkit-box-shadow: 0 0 15px rgba(0,0,0,.28), inset 0 1px 1px rgba(255,255,255,.45), inset 0 0 2px rgba(255,255,255,.2);   box-shadow: 0 0 15px rgba(0,0,0,.28), inset 0 1px 1px rgba(255,255,255,.45), inset 0 0 2px rgba(255,255,255,.2);   padding: 15px;   background-image: -webkit-linear-gradient(0deg,#ffffff 0%,#45bbba 50%,#ffffff 100%); }#this>.section_body { background: white; }#this>.section_body::-webkit-scrollbar { width: 5px; height: 5px;}#this>.section_body::-webkit-scrollbar-thumb {   border-radius: 5px;   background: #BBB; }#this>.section_body::-webkit-scrollbar-track {   background: #111; }]]></Css>            </Style>            <Width t="img.FixedWidth" Width="450"/>            <Height t="img.FixedHeight">              <Height t="ui.ScreenSize" Axis="height" Margin="100"/>            </Height>          </FieldAspect>          <Field t="control.Layout" ID="_Group1" Title="Group">            <Field t="control.Page" ID="_Link_to_page2" Title="Link to page">              <Page t="lmc.Details"/>            </Field>            <Layout t="layout.Default"/>          </Field>          <Layout t="layout.Default"/>          <FieldAspect t="field_aspect.FieldData" FieldData="%$CleanedItem%"/>        </Field>        <FieldAspect t="field_aspect.Variable" VarName="CleanedItem">          <Value t="xml.Xml">            <item id="1" name="Item 1">              <paragraph id="1" type="image" text="" image="//cdn.morguefile.com/imageData/public/files/k/karpati/preview/fldr_2009_12_25/file5251261764403.jpg" html="" imageWidth="" imageHeight="" video="" zoomByClick=""/>            </item>          </Value>        </FieldAspect>      </xtml>    </Component>  <Component id="LMCEditorItemChanged" type="action.Action">    <Param name="ID" />    <xtml t="object.RunMethod" Object="%$_AppContext%" Method="LMCEditorItemChanged" Input="%$ID%" />  </Component></xtml>