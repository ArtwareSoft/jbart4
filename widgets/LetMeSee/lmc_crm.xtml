<xtml ns="lmc" plugin="letmesee" altc="true">

<Component id="EditMetadata" type="jbart.MyWidgetPage">  <xtml t="control.Layout" ID="lmc_edit_metadata" Title="EditMetadata">    <Field t="field.ItemListContainer" ID="fields_cntr" Title="Additional fields for agent">      <Items t="itemlist.XmlItems" ParentXml="%%" Tag="field"/>      <Field t="field.ItemList" Title="fields" ID="fields_table">        <View t="itemlist_style.JBartTable" MinWidth="507px" Padding="5px"/>        <FieldAspect t="itemlist_aspect.ItemSelection"/>        <FieldAspect t="itemlist_aspect.TableColumnDragAndDrop"/>        <FieldAspect t="itemlist_aspect.DragAndDropItems"/>        <FieldAspect t="itemlist_aspect.ShowTextWhenNoItems" Text="No fields defined">          <Style t="text_as_item.Default"/>        </FieldAspect>        <FieldAspect t="itemlist_aspect.UpdateOnAddOrDeleteItem"/>        <Field t="fld.Field" FieldData="%!@name%" ID="name" Title="Field Name">          <FieldType t="fld_type.Text">            <Style t="ui.CustomCss" base="uitext.PlainText">              <Style t="uitext.PlainText"/>              <Css><![CDATA[#this { font-size: 14px; }]]></Css>            </Style>          </FieldType>        </Field>        <Field t="control.Layout" ID="_Group" Title="Type">          <Field t="fld.Field" FieldData="%!@type%" ID="type" Title="Type">            <FieldType t="fld_type.Picklist" AllowEmptyValue="" AllowValueNotInOptions="">              <Options t="editable_picklist.OptionsByCommas" Options="text,picklist"/>              <Style t="editable_picklist.LMCPicklist" Width="150px"/>            </FieldType>            <FieldAspect t="field_aspect.RefreshDependentFields" FieldsIds="extra" RefreshOn="every click" RefreshScope="screen"/>          </Field>          <Layout t="layout.Default"/>          <Field t="control.Layout" ID="extra" Title="Extra">            <Layout t="layout.Vertical" Spacing="0"/>            <Field t="control.PropertySheet" ID="_Property_Sheet" Title="Property Sheet">              <Style t="properties.TitlesAboveFields" HorizontalSpacing="15px" VerticalSpacing="8px"/>              <Field t="fld.Field" FieldData="%!@options%" ID="options" Title="Options (by commas)">                <FieldType t="fld_type.EditableText">                  <Style t="editable_text.LMCTextbox" Width="100%"/>                </FieldType>                <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%@type% == 'picklist' || %@type% == 'multi picklist'"/>              </Field>              <FieldAspect t="field_feature.Layout" dragProperty="#this margin">                <Css><![CDATA[#this { margin-top: 6px; margin-left: 0; }
#wrapper { }
]]></Css>              </FieldAspect>            </Field>          </Field>        </Field>      </Field>      <Field t="field.Layout" ID="fields_cntr_Toolbar" Title="Toolbar">        <Layout t="layout.HorizontalHtmlTable" Spacing="5px" VerticalAlign="top"/>        <FieldAspect t="itemlist_aspect.RefreshOnSelectionChange"/>        <FieldAspect t="field_feature.Layout">          <Css><![CDATA[#this { margin-top: 5px; }]]></Css>        </FieldAspect>        <Field t="control.Button" ID="fields_cntr_Add" Title="Add field">          <Style t="btn.HyperlinkWithImage"/>          <Image t="img.Image" Url="_jbartImages_/add1616.gif" OriginalWidth="16" OriginalHeight="16">            <AdjustSize t="img.Center"/>          </Image>          <Action t="popup.OpenPopup" PopupTitle="Add field for customer">            <Style t="popup.LMCDialog"/>            <Contents t="field.InnerPage">              <Layout t="layout.Default"/>              <Field t="control.Layout" ID="_Group" Title="Group">                <Field t="control.PropertySheet" ID="_Property_Sheet" Title="Property Sheet">                  <Style t="properties.LMCDialogPropertySheet"/>                  <Field t="fld.Field" FieldData="%!@name%" ID="name" Title="Field Name">                    <FieldType t="fld_type.EditableText">                      <Style t="editable_text.LMCTextbox" Width="252px"/>                    </FieldType>                    <FieldAspect t="field_aspect.Mandatory"/>                  </Field>                </Field>                <Field t="control.Layout" ID="_Group" Title="buttons">                  <Field t="control.Button" ID="_Button" Title="Add">                    <Style t="btn.LMCButton" Width="120px"/>                    <Action t="popup.CloseContainingPopup" ExitMode="OK">                      <DoOnExit t="itemlist_action.AddItemToItemList">                        <Location t="itemlist_action.AsLastItem"/>                      </DoOnExit>                    </Action>                  </Field>                  <Field t="control.Button" ID="_Button1" Title="Cancel">                    <Style t="btn.LMCGreyButton" Width="120px"/>                    <Action t="popup.CloseContainingPopup" ExitMode="Cancel"/>                  </Field>                  <Layout t="layout.DialogButtonsRightAlign"/>                </Field>                <Layout t="layout.Vertical" Spacing="20px"/>                <FieldAspect t="field_aspect.FieldData">                  <FieldData t="xml.Xml">                    <field/>                  </FieldData>                </FieldAspect>              </Field>            </Contents>          </Action>        </Field>        <Field t="control.Button" ID="fields_cntr_Delete" Title="Delete field">          <Style t="btn.HyperlinkWithImage"/>          <Image t="img.Image" Url="_jbartImages_/delete1616.gif" OriginalWidth="16" OriginalHeight="16">            <AdjustSize t="img.Center"/>          </Image>          <Action t="popup_confirmation.OpenConfirmationPopup" Title="Delete field">            <Action t="itemlist_action.DeleteSelectedItem"/>            <Style t="popup_confirmation.GoogleDriveConfirmation"/>          </Action>        </Field>      </Field>      <Layout t="layout.Default"/>      <SectionStyle t="section.DefaultNoUnderline"/>      <FieldAspect t="field_feature.Layout" dragProperty="#this margin">        <Css><![CDATA[#this { margin-top: 13px; margin-left: 0; }

]]></Css>      </FieldAspect>    </Field>    <Layout t="layout.Default"/>    <Field t="field.ItemListContainer" ID="fields_cntr1" Title="Status">      <Items t="itemlist.XmlItems" ParentXml="%!statuses%" Tag="status"/>      <Field t="field.ItemList" Title="statuses" ID="fields_table">        <View t="itemlist_style.JBartTable" MinWidth="360px"/>        <FieldAspect t="itemlist_aspect.ItemSelection" KeyboardSupport="true" AlwaysOneSelected="true" MouseSupport="mouse click">          <Css><![CDATA[{ background: #D9E8FB; }]]></Css>        </FieldAspect>        <FieldAspect t="itemlist_aspect.TableColumnDragAndDrop"/>        <FieldAspect t="itemlist_aspect.DragAndDropItems"/>        <FieldAspect t="itemlist_aspect.ShowTextWhenNoItems" Text="No statuses defined">          <Style t="text_as_item.Default"/>        </FieldAspect>        <FieldAspect t="itemlist_aspect.UpdateOnAddOrDeleteItem"/>        <Field t="fld.Field" FieldData="%!@id%" ID="name" Title="Status">          <FieldType t="fld_type.Text">            <Style t="ui.CustomCss" base="uitext.PlainText">              <Style t="uitext.PlainText"/>              <Css><![CDATA[#this { font-size: 14px; }
]]></Css>            </Style>          </FieldType>          <FieldAspect t="field_feature.Css">            <Css><![CDATA[#wrapper { width: 507px; }
]]></Css>          </FieldAspect>        </Field>      </Field>      <Field t="field.Layout" ID="fields_cntr_Toolbar" Title="Toolbar">        <Layout t="layout.HorizontalHtmlTable" Spacing="5px" VerticalAlign="top"/>        <FieldAspect t="itemlist_aspect.RefreshOnSelectionChange"/>        <FieldAspect t="field_feature.Layout">          <Css><![CDATA[#this { margin-top: 5px; }]]></Css>        </FieldAspect>        <Field t="control.Button" ID="fields_cntr_Add" Title="Add status">          <Style t="btn.HyperlinkWithImage"/>          <Image t="img.Image" Url="_jbartImages_/add1616.gif" OriginalWidth="16" OriginalHeight="16">            <AdjustSize t="img.Center"/>          </Image>          <Action t="popup.OpenPopup" PopupTitle="Add status">            <Style t="popup.LMCDialog"/>            <Contents t="field.InnerPage">              <Layout t="layout.Default"/>              <Field t="control.Layout" ID="_Group" Title="Group">                <Field t="control.PropertySheet" ID="_Property_Sheet" Title="Property Sheet">                  <Style t="properties.LMCDialogPropertySheet"/>                  <Field t="fld.Field" FieldData="%!@id%" ID="name" Title="Status">                    <FieldType t="fld_type.EditableText">                      <Style t="editable_text.LMCTextbox" Width="252px"/>                    </FieldType>                    <FieldAspect t="field_aspect.Mandatory"/>                    <FieldAspect t="field_feature.ClickOKOnEnter" OKButtonID="Add"/>                  </Field>                </Field>                <Field t="control.Layout" ID="_Group" Title="buttons">                  <Field t="control.Button" ID="Add" Title="Add" HideTitle="true">                    <Style t="btn.LMCButton" Width="120px"/>                    <Action t="popup.CloseContainingPopup" ExitMode="OK">                      <DoOnExit t="itemlist_action.AddItemToItemList">                        <Location t="itemlist_action.AsLastItem"/>                        <DoOnAddedItem t="field.RefreshField" FireOnUpdate="" FieldID="status_mapper" Scope="screen"/>                      </DoOnExit>                    </Action>                  </Field>                  <Field t="control.Button" ID="_Button1" Title="Cancel">                    <Style t="btn.LMCGreyButton" Width="120px"/>                    <Action t="popup.CloseContainingPopup" ExitMode="Cancel"/>                  </Field>                  <Layout t="layout.DialogButtonsRightAlign"/>                </Field>                <Layout t="layout.Vertical" Spacing="20px"/>                <FieldAspect t="field_aspect.FieldData">                  <FieldData t="xml.Xml">                    <status/>                  </FieldData>                </FieldAspect>              </Field>            </Contents>          </Action>        </Field>        <Field t="control.Button" ID="fields_cntr_Delete" Title="Delete status">          <Style t="btn.HyperlinkWithImage"/>          <Image t="img.Image" Url="_jbartImages_/delete1616.gif" OriginalWidth="16" OriginalHeight="16">            <AdjustSize t="img.Center"/>          </Image>          <Action t="popup_confirmation.OpenConfirmationPopup" Title="Delete status">            <Action t="itemlist_action.DeleteSelectedItem"/>            <Style t="popup_confirmation.GoogleDriveConfirmation"/>          </Action>        </Field>      </Field>      <Layout t="layout.Default"/>      <SectionStyle t="section.DefaultNoUnderline"/>      <FieldAspect t="field_feature.Layout" dragProperty="#this margin">        <Css><![CDATA[#this { margin-top: 15px; margin-left: 0; }

]]></Css>      </FieldAspect>    </Field>  </xtml></Component>

</xtml>