<xtml ns="lmc" plugin="letmesee" altc="true"><Component id="Details" type="field.Fields">  <Param name="HideNavigationButtons" type="data.Boolean"/>  <xtml t="control.Layout" ID="Details" Title="Details">    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="right_to_left" ConditionForClass="%$Room/style/general/@rtl%"/>    <Layout t="layout.Default"/>    <Field t="control.Layout" ID="Item_topbar" Title="Top Bar">      <Field t="control.Button" ID="Button" Title="All" HideTitle="true" ButtonText="">        <Style t="ui.CustomStyle" base="btn.Hyperlink">          <Html><![CDATA[<div>  <div class="dropdown" /></div>]]></Html>          <Css><![CDATA[#this {  position:absolute;  cursor:pointer;  z-index:2;  -webkit-tap-highlight-color:transparent;}]]></Css>          <Javascript><![CDATA[function render(button) {  button.$el.click(function(e) {     button.$el.attr('title',button.text);        aa_buttonRunAction(button,e);  }); }]]></Javascript>        </Style>        <Action t="action.RunActions">          <Action t="itemlist_action.CloseDetailsReplacingAll" AutoScrollToMasterItem="false">            <Transition t="lmc.LMCSlideTransition" Direction="From Left" Duration="250"/>          </Action>          <Action t="object.RunMethod" Object="%$_AppContext%" Method="LMCShowingItem"/>        </Action>        <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="topbar_button topbar_back_button"/>        <FieldAspect t="field_aspect.Hidden">          <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$HideNavigationButtons% != 'true'"/>        </FieldAspect>      </Field>      <Layout t="ui.CustomStyle" base="layout.Default">        <Html><![CDATA[<div><div class="field"/></div>]]></Html>        <Css><![CDATA[#this {  position:relative;  width:100\%;  z-index:10;}.topbar_button {  position:absolute;  cursor:pointer;}.dropdown {  position:absolute;  top:0;}.dropdown:before, .dropdown:after {  position:absolute;  content:"";  top:0;}]]></Css>        <Javascript><![CDATA[function(layout) {  aa_layout(layout);  if ( '%$VisitorDesktopLook%' != 'true' ) {    aa_positionFixed(layout.el,{  top: 0, addMarginToParent: false, object: layout, disableZoom:true });  }}]]></Javascript>      </Layout>      <Field t="control.Button" ID="Button" Title="Next Item">        <Disabled t="yesno.ConditionByXtml">          <Expression t="yesno.IsEmpty">            <Value t="itemlist_action.NextItemOfDetailsReplacingAll">              <FilterForNextItem t="yesno.And">                <Item value="%@type% != 'heading'"/>                <Item t="xml.IsOfTag" Tag="item"/>              </FilterForNextItem>            </Value>          </Expression>        </Disabled>        <Style t="ui.CustomStyle" base="btn.Hyperlink">          <Html><![CDATA[<div>  <div class="dropdown" /></div>]]></Html>          <Css><![CDATA[#this {  z-index:2;  -webkit-tap-highlight-color: transparent;}]]></Css>          <Javascript><![CDATA[function render(button) {  button.$el.click(function(e) {     button.$el.attr('title',button.text);        aa_buttonRunAction(button,e);  }); }]]></Javascript>        </Style>        <Action t="async.RunOnNextTimer" Milliseconds="1">          <Action t="action.RunActions">            <Action t="itemlist_action.DetailsReplacingAllGotoNextItem">              <FilterForNextItem t="yesno.And">                <Item value="%@type% != 'heading'"/>                <Item t="xml.IsOfTag" Tag="item"/>              </FilterForNextItem>            </Action>            <Action t="object.RunMethod" Object="%$_AppContext%" Method="LMCShowingItem"/>          </Action>        </Action>        <FieldAspect t="field_aspect.Hidden">          <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$HideNavigationButtons% != 'true'"/>        </FieldAspect>        <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="topbar_button topbar_next_button"/>      </Field>      <Field t="control.Button" ID="topbar_previtem" Title="Previous Item" HideTitle="true">        <Style t="ui.CustomStyle" base="btn.Hyperlink">          <Html><![CDATA[<div>  <div class="dropdown" /></div>]]></Html>          <Css><![CDATA[#this {  z-index:2;  -webkit-tap-highlight-color: transparent;}]]></Css>          <Javascript><![CDATA[function render(button) {  button.$el.attr('title',button.text);  button.$el.click(function(e) {     aa_buttonRunAction(button,e);  }); }]]></Javascript>        </Style>        <Action t="async.RunOnNextTimer" Milliseconds="1">          <Action t="action.RunActions">            <Action t="itemlist_action.DetailsReplacingAllGotoPrevItem">              <FilterForPrevItem t="yesno.And">                <Item value="%@type% != 'heading'"/>                <Item t="xml.IsOfTag" Tag="item"/>              </FilterForPrevItem>            </Action>            <Action t="object.RunMethod" Object="%$_AppContext%" Method="LMCShowingItem"/>          </Action>        </Action>        <FieldAspect t="field_aspect.Hidden">          <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%$HideNavigationButtons% != 'true'"/>        </FieldAspect>        <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="topbar_button topbar_prev_button"/>        <Disabled t="yesno.ConditionByXtml">          <Expression t="yesno.IsEmpty">            <Value t="itemlist_action.PrevItemOfDetailsReplacingAll">              <FilterForPrevItem t="yesno.And">                <Item value="%@type% != 'heading'"/>                <Item t="xml.IsOfTag" Tag="item"/>              </FilterForPrevItem>            </Value>          </Expression>        </Disabled>      </Field>      <FieldAspect t="lmc.CssFromSettings" CssID="details topbar"/>      <Field t="control.Label" ID="_Label5" Title="Title">        <Style t="ui.CustomCss" base="uitext.PlainText">          <Style t="uitext.PlainText"/>          <Css><![CDATA[#this {  position:absolute;  top:0;  vertical-align:center;  text-overflow: ellipsis;  overflow: hidden;  white-space: nowrap;  text-align:center;}]]></Css>        </Style>        <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="item_title"/>        <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="long_item_title">          <ConditionForClass t="yesno.GreaterThan" Than="30">            <Item t="text.Length" Data="%@name%"/>          </ConditionForClass>        </FieldAspect>        <Text t="data.Pipeline">          <Item value="%@name%"/>          <Item t="lmc.ReplaceVariableFields"/>        </Text>        <FieldAspect t="field_aspect.CssOnCondition" ConditionInput="item">          <Css><![CDATA[#this {  direction: %@direction% !important; }]]></Css>          <OnCondition t="yesno.Expression" Expression="%@direction% != ''"/>        </FieldAspect>      </Field>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="item_topbar"/>    </Field>    <Field t="control.Layout" ID="_Group887" Title="Contents">      <Layout t="ui.CustomCss" base="layout.Default">        <Style t="layout.Default"/>        <Css><![CDATA[#this {}]]></Css>      </Layout>      <Field t="field.Image" ID="_Image1" Title="Main Image">        <Image t="img.Image" Url="%@Image%" OriginalWidth="%@imageWidth%" OriginalHeight="%@imageHeight%">          <FrameWidth t="img.ContainerWidth" Margin=""/>          <AdjustSize t="img.Center"/>        </Image>        <Style t="image.PlainImage"/>        <FieldAspect t="field_feature.Layout">          <Css><![CDATA[#this {  padding-top:5px;  padding-bottom:20px;}#wrapper {}]]></Css>        </FieldAspect>        <FieldAspect t="field_aspect.Hidden">          <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%@showMainImageInDetails%"/>        </FieldAspect>      </Field>      <Field t="field.ItemList" Title="paragraphs" ID="paragraphs_table1">        <View t="ui.CustomCss" base="itemlist_style.Simple">          <Style t="itemlist_style.Simple"/>          <Css><![CDATA[#this>.aa_item {}#this{}]]></Css>        </View>        <FieldAspect t="field_aspect.ItemListContainer">          <Items t="itemlist.Items">            <Items t="data.Pipeline">              <Item value="%paragraph%"/>              <Item t="yesno.PassesFilter">                <Filter t="yesno.OR">                  <Item value="%@type% != 'property sheet'"/>                  <Item t="yesno.Not">                    <Of t="data.IsInCommaSeparatedList" List="%@aspects%" Item="show only in price quote"/>                  </Item>                </Filter>              </Item>            </Items>          </Items>        </FieldAspect>        <Field t="control.Page" ID="_Link_to_page5" Title="Paragraph">          <Page t="lmc.LMCRenderParagraph" InMainMenu="" InVisitorDesktop=""/>        </Field>      </Field>      <Field t="field.ItemList" Title="properties" ID="paragraphs_table1">        <View t="itemlist_style.JBartTableNoHeader" MinWidth=""/>        <FieldAspect t="field_aspect.ItemListContainer">          <Items t="itemlist.Items" Items="%property%"/>        </FieldAspect>        <Field t="control.Label" ID="specification_name" Title="Name" Text="%@name%">          <Style t="ui.CustomCss" base="uitext.PlainText">            <Style t="uitext.PlainText"/>            <Css><![CDATA[#this {}]]></Css>          </Style>          <FieldAspect t="field_feature.CssClass" ClassName="spec_name" AddClassTo="wrapper"/>        </Field>        <Field t="control.Label" ID="_Label4" Title="Value" Text="%@value%">          <Style t="uitext.PlainText"/>          <FieldAspect t="field_feature.CssClass" AddClassTo="wrapper" ClassName="spec_value"/>        </Field>        <FieldAspect t="lmc.CssFromSettings" CssID="specification"/>        <FieldAspect t="field_feature.Css">          <Css><![CDATA[.right_to_left #this {  direction: rtl;  text-align:right;}]]></Css>        </FieldAspect>        <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Do not render">          <ShowCondition t="yesno.And">            <Item t="yesno.NotEmpty" Value="%property%"/>          </ShowCondition>        </FieldAspect>      </Field>      <FieldAspect t="lmc.CssFromSettings" CssID="item contents"/>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="item_contents"/>    </Field>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="lmc_item_details"/>    <FieldAspect t="lmc.MinHeightForViralItem" BottomPadding="68"/>    <FieldAspect t="lmc.CssFromSettings" CssID="item contents"/>  </xtml></Component><Component id="ReplaceVariablesWhenInstantiatingTemplate" type="data.Data" execution="native" /><Component id="ReplaceVariableFields" type="data.Data">  <Param name="RoomXml" Default="%$Room%" />  <xtml t="data.Pipeline">    <Item t="text.Replace" Find="{customer name}">      <ReplaceWith t="text.FirstSucceeding">        <Item value="%$RoomXml/@customerName%"/>        <Item value="xxx" Condition="%$RoomXml/@isTemplate%" />      </ReplaceWith>    </Item>    <Item t="text.Replace" Find="{customer first name}">      <ReplaceWith t="text.FirstSucceeding">        <Item value="xxx" Condition="%$RoomXml/@isTemplate%" />        <Item t="text.Split" Text="%$RoomXml/@customerName%" Separator=" " Part="First" />      </ReplaceWith>    </Item>    <Item t="text.Replace" Find="{customer phone}">      <ReplaceWith t="text.FirstSucceeding">        <Item value="xxx" Condition="%$RoomXml/@isTemplate%" />        <Item value="%$RoomXml/@phone%"  />      </ReplaceWith>    </Item>    <Item t="text.Replace" Find="{customer company name}">      <ReplaceWith t="text.FirstSucceeding">        <Item value="xxx" Condition="%$RoomXml/@isTemplate%" />        <Item value="%$RoomXml/@customerCompany%" />      </ReplaceWith>    </Item>    <Item t="text.Replace" Find="{agent name}">      <ReplaceWith t="text.FirstSucceeding">        <Item value="xxx" Condition="%$RoomXml/@isTemplate%" />        <Item value="%$RoomXml/@agent%"/>      </ReplaceWith>    </Item>    <Item t="text.Replace" Find="{agent first name}">      <ReplaceWith t="text.FirstSucceeding">        <Item value="xxx" Condition="%$RoomXml/@isTemplate%" />        <Item t="text.Split" Text="%$RoomXml/@agent%" Separator=" " Part="First" />      </ReplaceWith>    </Item>    <Item t="text.Replace" Find="{agent phone}">      <ReplaceWith t="text.FirstSucceeding">        <Item value="%$RoomXml/@agentPhone%" />        <Item value="000-0000000" Condition="%$RoomXml/@isTemplate%" />      </ReplaceWith>    </Item>    <Item t="text.Replace" Find="{agent email}">      <ReplaceWith t="text.FirstSucceeding">        <Item value="%$RoomXml/@agentEmail%" />        <Item value="agent email" Condition="%$RoomXml/@isTemplate%" />      </ReplaceWith>    </Item>    <Item t="text.Replace" Find="{agent phone link}">      <ReplaceWith t="text.FirstSucceeding">        <Var name="PhoneFixed" t="text.Replace" Text="%$RoomXml/@agentPhone%" Find="-" ReplaceWith=""/>        <Item value="000-0000000" Condition="%$RoomXml/@isTemplate%" />        <Item value="&lt;a href=&quot;tel:%$PhoneFixed%&quot;&gt;%$RoomXml/@agentPhone%&lt;/a&gt;"/>      </ReplaceWith>    </Item>    <Item t="text.Replace" Find="{agent email link}">      <ReplaceWith t="text.FirstSucceeding">        <Item value="agent email" Condition="%$RoomXml/@isTemplate%" />        <Item value="&lt;a href=&quot;mailto:%$RoomXml/@agentEmail%&quot;&gt;%$RoomXml/@agentEmail%&lt;/a&gt;" />      </ReplaceWith>    </Item>  </xtml></Component></xtml>