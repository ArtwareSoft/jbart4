<xtml ns="lmc_manager" altc="true">  <Component id="SaveLMCManagerSettings" type="action.Action" execution="native" />  <Component id="SaveSiteSettingsFromManager" type="action.Action" execution="native" />	<Component id="LoadSiteSettingsFromManager" type="action.Action" execution="native" />		<Component id="InitLMCManager" type="action.Action" execution="native" />  <Component id="CreateProjectAndUserFromManager" type="action.Action">    <Param name="ProjectName" />    <Param name="AlsoCreateUser" type="data.Boolean" />    <Param name="UserEmail" />    <Param name="UserName" />    <Param name="UserPhone" />    <Param name="UserWorkEmail" />    <Param name="UserRemark" />    <Param name="AllowedUsers" />    <xtml t="async.RunAsyncActions">      <Var name="NewProjectID" t="data.WritableText" />      <Var name="AllowedUsers2" t="data.IfThenElse" If="%$AlsoCreateUser%" Else="%$AllowedUsers%">        <Then t="data.AddToCommaSeparated" List="%$AllowedUsers%" ToAdd="%$UserEmail%" />      </Var>      <Action t="xml.AddXmlChildren" Condition="%$AlsoCreateUser%" Parent="%$SiteFile/!users%">        <Children t="xml.Xml" DynamicContent="true">          <user id="%$UserEmail%" name="%$UserName%" phoneNumber="%$UserPhone%" workEmail="%$UserWorkEmail%" remark="%$UserRemark%"/>        </Children>      </Action>      <Action t="lmc_client.CreateProjectFromClient" ClientID="%$Projects/@site%" ClientSettings="%$SiteFile%" ProjectName="%$ProjectName%" AllowedUsers="%$AllowedUsers2%">        <DoOnNewProject t="action.RunActions">          <Action t="action.WriteValue" To="%$NewProjectID%" Value="%@id%" />        </DoOnNewProject>      </Action>      <Action t="lmc_manager.SaveSiteSettingsFromManager" />      <Action t="lmc_manager.NotifyOnNewProjectCreated" ProjectID="%$NewProjectID%" />     </xtml>  </Component>  <Component id="NotifyOnNewProjectCreated" type="action.Action">    <Param name="ProjectID" />    <xtml t="popup.OpenPopup" PopupTitle="Project Created">      <Style t="popup.LMCDialog"/>      <Contents t="field.InnerPage">      <Field t="control.Layout" ID="popup_buttons" Title="Buttons">        <Layout t="layout.DialogButtonsRightAlign"/>        <Field t="control.Button" ID="OK" Title="Open project" HideTitle="true">          <Style t="btn.LMCButton" Width="140px"/>          <Action t="popup.CloseContainingPopup" ExitMode="OK">            <DoOnExit t="operation.Link" OpenInANewTab="true" Link="https://jb-letmesee.appspot.com/letmesee.html?project=%$ProjectID%"/>          </Action>        </Field>        <Field t="control.Button" ID="OK" Title="Close" HideTitle="true">          <Style t="btn.LMCGreyButton" Width="140px"/>          <Action t="popup.CloseContainingPopup" ExitMode="OK">            <DoOnExit t="action.RunActions"/>          </Action>        </Field>        <FieldAspect t="field_feature.Layout">          <Css><![CDATA[#this { margin-top: 20px; }]]></Css>        </FieldAspect>      </Field>              </Contents>    </xtml>  </Component>	<Component id="AddLMCProjectToManagerSettings" type="action.Action" execution="native">		<Param name="OnAdded" type="action.Action" script="true" />	</Component>	<Component id="AddLMCProjectToClientSettings" type="action.Action" execution="native">		<Param name="OnAdded" type="action.Action" script="true" />	</Component><Component id="ShowRoomFromHeaders" type="action.Action">  <xtml t="action.IfThenElse" If="%$IsMobile%">    <Then t="operation.Link" OpenInANewTab="true" >      <Link t="data.Pipeline">        <Item value="https://jb-letmesee.appspot.com/letmesee.html?project=%../../@id%#?roomid=%@id%"/>      </Link>  </Then>  <Else t="popup.OpenPopup" PopupTitle="%@customerName% - %../../@name%">    <Style t="popup.LMCDialog"/>    <Contents t="field.InnerPage">      <Layout t="layout.Default"/>      <Field t="control.IFrame" ID="_IFrame" Title="IFrame">        <Style t="ui.CustomStyle" base="iframe_style.Simple">          <Html><![CDATA[<iframe />]]></Html>          <Css><![CDATA[#this {  width: 300px;  height: 300px;        }]]></Css>          <Javascript><![CDATA[function render(iframe) {   iframe.el.setAttribute('src',iframe.url);  if (iframe.params.Border != 'true') iframe.el.setAttribute('frameborder','false');  iframe.$el.width( screen.width - 300);  iframe.$el.height( screen.height - 300);}]]></Javascript>        </Style>        <Url t="data.Pipeline">          <Item value="%$UrlPrefix%?type=room_feed&amp;room=%@id%&amp;project=%$ProjectID%&amp;key=%$Key%&amp;manager=true"/>          <Var name="ProjectID" value="%../../@id%"/>          <Var name="Key" value="%$Projects/project[#{$ProjectID}]/@key%"/>          <Var name="UrlPrefix" value="https://jb-letmesee.appspot.com/letmesee_embed.html"/>        </Url>        <FieldAspect t="field_feature.HandleEvent">          <Event t="field_feature.Load"/>          <Action t="lmc.TrackUserAction" Type="manager_show_room" Text="room=%@customerName%"/>        </FieldAspect>      </Field>      <Field t="control.Layout" ID="popup_buttons" Title="Buttons">        <Layout t="layout.DialogButtonsRightAlign"/>        <Field t="control.Button" ID="OK1" Title="Open in a new tab" HideTitle="true">          <Style t="btn.LMCButton" Width="167px"/>          <Action t="popup.CloseContainingPopup" ExitMode="OK">            <DoOnExit t="operation.Link" OpenInANewTab="true">              <Link t="data.Pipeline">                <Item value="https://jb-letmesee.appspot.com/letmesee.html?project=%../../@id%#?roomid=%@id%"/>              </Link>            </DoOnExit>          </Action>        </Field>        <Field t="control.Button" ID="OK" Title="Close" HideTitle="true">          <Style t="btn.LMCGreyButton" Width="140px"/>          <Action t="popup.CloseContainingPopup" ExitMode="OK">            <DoOnExit t="action.RunActions"/>          </Action>        </Field>        <FieldAspect t="field_feature.Layout">          <Css><![CDATA[#this { margin-top: 20px; }]]></Css>        </FieldAspect>      </Field>    </Contents>  </Else>  </xtml></Component><Component id="ProspectsLoad" type="action.Action" execution="native" /><Component id="LoadProspect" type="action.Action" execution="native">  <Param name="ProspectFile" /></Component><Component id="SaveProspect" type="action.Action" execution="native">  <Param name="ProspectFile" /></Component><Component id="AddNewProspect" type="action.Action" execution="native">  <Param name="Name" />  <Param name="Permissions" /></Component><Component id="ProspectUpdatePermissions" type="action.Action" execution="native">  <Param name="ProspectFile" />  <Param name="NewPermissions" /></Component><Component id="SuperAdminRunLS" type="action.Action" execution="native">  <Param name="Prefix" /></Component>  <Component id="DateTimeFromGoogleCloudStorageFormat" type="data.Data" execution="native" /><Component id="AgentTrackingFileToSessions" type="data.Data" execution="native">  <Param name="Lines" /></Component><Component id="ManagerEMailBody" type="data.Data" execution="native">  <Param name="ManagerEmailXml" /></Component><Component id="IsUserTrackingInDateRange" type="data.Boolean" execution="native">  <Param name="DaysBack" />  <Param name="DateString" /></Component><Component id="AgentPerformance" type="data.Data" execution="native">  <Param name="Rooms" />  <Param name="DaysBack" Default="7"/></Component><Component id="DateFilterOptions" type="data.Data" execution="native" /></xtml>