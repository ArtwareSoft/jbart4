<xtml ns="lmc" altc="true"><Component id="MobileView" type="jbart.MyWidgetPage">  <Param name="DoNotShowDetails" type="data.Boolean"/>  <Param name="OnSelectItem" type="action.Action" script="true"/>  <xtml t="control.TabControl" ID="ui_states" Title="UI States ">    <FieldAspect t="lmc.MinHeightForViralItem" BottomPadding="68"/>    <Style t="tabs.HiddenTabs"/>    <Field t="control.Layout" ID="MainTab" Title="Main">      <Layout t="ui.CustomCss" base="layout.Default">        <Style t="layout.Default"/>        <Css><![CDATA[#this {}]]></Css>      </Layout>      <Field t="field.ItemListContainer" ID="items_cntr" Title="items">        <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="right_to_left" ConditionForClass="%$Room/style/general/@rtl%"/>        <Items t="itemlist.Items">          <Items t="lmc.MainItems"/>        </Items>        <Field t="field.ItemList" Title="items" ID="items_table1">          <View t="ui.CustomCss" base="itemlist_style.Simple">            <Style t="itemlist_style.Simple"/>            <Css><![CDATA[.aa_item { -webkit-tap-highlight-color: transparent;}]]></Css>          </View>          <Field t="control.Layout" ID="_Group3" Title="Group">            <Field t="control.Layout" ID="_ItemHeading" Title="Heading">              <Layout t="layout.Default"/>              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%@type% == 'heading'"/>              </FieldAspect>              <FieldAspect t="lmc.CssFromSettings" CssID="heading"/>              <Field t="fld.Field" ID="name" Title="Name">                <FieldType t="fld_type.Text">                  <Style t="ui.CustomCss" base="uitext.PlainText">                    <Style t="uitext.PlainText"/>                    <Css><![CDATA[.right_to_left #this {  direction: rtl;}]]></Css>                  </Style>                </FieldType>                <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="heading_text"/>                <FieldData t="data.Pipeline">                  <Item value="%@name%"/>                  <Item t="lmc.ReplaceVariableFields"/>                </FieldData>              </Field>            </Field>            <Layout t="layout.Default"/>            <Field t="control.Layout" ID="_Vertical_Layout" Title="Item">              <Layout t="ui.CustomCss" base="layout.Vertical">                <Style t="layout.Vertical" Spacing="0"/>                <Css><![CDATA[#this {  cursor:  pointer;  position: relative;}#this >.field:not(:last-child) {  margin-bottom: 0;}]]></Css>              </Layout>              <Field t="fld.Field" ID="name" Title="Name">                <FieldType t="fld_type.Text">                  <Style t="ui.CustomCss" base="uitext.PlainText">                    <Style t="uitext.PlainText"/>                    <Css><![CDATA[.right_to_left #this {  direction:rtl;}]]></Css>                  </Style>                </FieldType>                <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="item_text"/>                <FieldAspect t="field_aspect.CssOnCondition" ConditionInput="item">                  <Css><![CDATA[#this {  direction: %@direction% !important; }]]></Css>                  <OnCondition t="yesno.Expression" Expression="%@direction% != ''"/>                </FieldAspect>                <FieldData t="data.Pipeline">                  <Item value="%@name%"/>                  <Item t="lmc.ReplaceVariableFields"/>                </FieldData>              </Field>              <Field t="control.Layout" ID="_Group4" Title="Image">                <Field t="field.Image" ID="_Image" Title="Image">                  <Image t="img.Image" Url="%@Image%" OriginalWidth="%@imageWidth%" OriginalHeight="%@imageHeight%">                    <AdjustSize t="data.FirstSucceeding">                      <Item t="img.Center" Condition="%@fit% == 'Center'"/>                      <Item t="img.Fill"/>                    </AdjustSize>                    <FrameWidth t="img.ContainerWidth" Margin="%$Room/style/general/@marginForImageInMainMenu%"/>                    <FrameHeight t="img.FixedHeight">                      <Height t="lmc.LMCImageHeight"/>                    </FrameHeight>                  </Image>                  <Style t="image.PlainImage"/>                  <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="item_image"/>                </Field>                <Layout t="layout.Default"/>                <Field t="control.CustomControl" ID="_Custom_control1" Title="Arrow">                  <Control t="ui.CustomStyle">                    <Html><![CDATA[<div>  <div class="item_arrow_cover" />  <div class="item_arrow" /></div>]]></Html>                    <Css><![CDATA[]]></Css>                    <Javascript><![CDATA[function render(object) {/* Use object.el or object.$el to change the element */  }]]></Javascript>                  </Control>                </Field>                <FieldAspect t="field_feature.Css">                  <Css><![CDATA[#this {  position: relative;}]]></Css>                </FieldAspect>              </Field>              <FieldAspect t="field_feature.Layout">                <Css><![CDATA[#this {  overflow: hidden;  position:relative;}#wrapper {}]]></Css>              </FieldAspect>              <FieldAspect t="field_aspect.Hidden">                <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render">                  <ShowCondition t="yesno.And">                    <Item value="%@type% != 'heading'"/>                    <Item t="xml.IsOfTag" Tag="item"/>                  </ShowCondition>                </OnCondition>              </FieldAspect>              <FieldAspect t="lmc.CssFromSettings" CssID="item"/>              <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="no_image" ConditionForClass="%@image% == ''"/>              <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="last_in_heading">                <ConditionForClass t="lmc.IsLastInHeading"/>              </FieldAspect>              <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="after_item">                <ConditionForClass t="yesno.And">                  <Var name="PrevItem" t="xml.PreviousSibling"/>                  <Item value="%$PrevItem/@type% != 'heading'"/>                  <Item value="%$PrevItem/@id% != ''"/>                </ConditionForClass>              </FieldAspect>              <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="lmc_item"/>            </Field>            <Field t="control.Layout" ID="_Vertical_Layout_P" Title="Paragraph">              <Layout t="layout.Vertical" Spacing="0"/>              <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Do not render">                <ShowCondition t="xml.IsOfTag" Tag="paragraph"/>              </FieldAspect>              <FieldAspect t="lmc.CssFromSettings" CssID="item"/>              <FieldAspect t="lmc.LMCParagraphParentInMainMenuFieldAspects"/>              <Field t="control.Page" ID="_Link_to_page5" Title="paragraph">                <Page t="lmc.LMCRenderParagraph" InMainMenu="true" InVisitorDesktop=""/>              </Field>            </Field>          </Field>          <FieldAspect t="lmc.LMCItemslist">            <Style t="cobrowse_style.DefaultMarkShownItem"/>            <ActionToShowItem t="itemlist_action.ClickOnItem" NoTransitions="true">              <Item t="itemlist_item.ItemInContext"/>            </ActionToShowItem>            <ActionToShowNoItem t="itemlist_action.CloseDetailsReplacingAll" AutoScrollToMasterItem="true"/>          </FieldAspect>          <FieldAspect t="itemlist_aspect.ItemClick">            <OnClick t="action.RunActions">              <Condition t="yesno.And">                <Item t="yesno.Not" Of="%$DoNotShowDetails%"/>                <Item value="%@type% != 'heading'"/>                <Item t="xml.IsOfTag" Tag="item"/>              </Condition>              <Action t="itemlist_action.OpenDetailsReplacingAll" ItemList="" FieldToReplace="">                <Details t="control.Page" Title="Details">                  <Page t="lmc.Details" HideNavigationButtons=""/>                </Details>                <Item t="itemlist_item.ItemInContext"/>                <Transition t="lmc.LMCSlideTransition" Direction="From Right" Duration="400"/>              </Action>              <Action t="object.RunMethod" Object="%$_AppContext%" Method="LMCShowingItem"/>              <Action t="xtml.UseParam" Param="OnSelectItem"/>            </OnClick>            <Css><![CDATA[#this:hover { cursor: pointer; } ]]></Css>          </FieldAspect>          <FieldAspect t="itemlist_aspect.LongList" MaxTimeToRenderItems="2000" MaxItemsPerPage="20" ShowAllItemsOnTimer="true">            <Style t="ui.CustomStyle" base="text_as_item.Default">              <Html><![CDATA[<div>  <div class="text" /></div>]]></Html>              <Css><![CDATA[#this {  cursor: pointer;  color:#fff;  clear: both;  width:100\%;  height: 80px;  background: #000;  line-height:80px;}.text {  margin:auto;  width:100\%;  text-align: center;  font-size: 16px;}]]></Css>              <Javascript><![CDATA[function (showMoreObject) {      $(showMoreObject.control).find('.text').text(showMoreObject.text);      showMoreObject.control.onclick = function() { showMoreObject.showMore() };}]]></Javascript>            </Style>          </FieldAspect>          <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="main_menu_items"/>          <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="lmc_main_menu"/>        </Field>        <Layout t="layout.Default"/>      </Field>      <FieldAspect t="field_feature.HandleEvent">        <Event t="field_feature.Load"/>        <Action t="lmc.AddAllCssFromSettings"/>      </FieldAspect>    </Field>  </xtml></Component><Component id="LMCParagraphParentInMainMenuFieldAspects" type="field.FieldAspect">  <xtml t="field_aspect.FieldAspects">    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="paragraph_in_main_menu"/>    <FieldAspect t="field_feature.Css">      <Css><![CDATA[#this { cursor: default; }]]></Css>    </FieldAspect>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="after_item">      <ConditionForClass t="yesno.And">        <Var name="PrevItem" t="xml.PreviousSibling"/>        <Item value="%$PrevItem/@type% != 'heading'"/>        <Item value="%$PrevItem/@id% != ''"/>      </ConditionForClass>    </FieldAspect>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="last_in_heading">      <ConditionForClass t="lmc.IsLastInHeading"/>    </FieldAspect>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="no_border" ConditionForClass="%@noBorder%"/>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="paragraph_text" ConditionForClass="%@type% == 'text'"/>  </xtml></Component><Component id="LMCRenderParagraph" type="jbart.MyWidgetPage">  <Param name="InMainMenu" type="data.Boolean"/>  <Param name="InVisitorDesktop" type="data.Boolean"/>  <Param name="InDesktopMasterArea" type="data.Boolean"/>  <xtml t="control.Layout" ID="_Group886" Title="Paragraph">    <Field t="control.Label" ID="paragraph_text" Title="Text">      <Style t="ui.CustomStyle" base="uitext.PlainText">        <Html><![CDATA[<div />]]></Html>        <Css><![CDATA[.right_to_left #this {  direction:rtl;  text-align:right;}]]></Css>        <Javascript><![CDATA[function(textObj) {  var text = aa_textAutoLinks(textObj.text);  aa_textLabel(textObj);}]]></Javascript>      </Style>      <FieldAspect t="field_aspect.Hidden">        <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render">          <ShowCondition t="yesno.And">            <Item t="yesno.NotEmpty" Value="%@text%"/>            <Item t="yesno.NotInList" Item="%@type%">              <List t="data.ListByCommas" List="html,rich text"/>            </Item>          </ShowCondition>        </OnCondition>      </FieldAspect>      <Text t="data.Pipeline">        <Item value="%@text%"/>        <Item t="lmc.ReplaceVariableFields"/>      </Text>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="paragraph_image_text" ConditionForClass="%@type% == 'image'"/>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="paragraph_grid_text" ConditionForClass="%@type% == 'grid'"/>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="paragraph_text"/>    </Field>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="%@cssClass%" ConditionForClass="%@cssClass% != ''"/>    <Layout t="ui.CustomStyle" base="layout.Default">      <Html><![CDATA[<div class="paragraph_outer">  <div class="paragraph_inner">    <div class="field"/>  </div></div>]]></Html>      <Css><![CDATA[#this {}]]></Css>      <Javascript><![CDATA[function(layout) {  aa_layout(layout);}]]></Javascript>    </Layout>    <Field t="control.Label" ID="paragraph_html" Title="Html">      <Style t="ui.CustomStyle" base="uitext.PlainText">        <Html><![CDATA[<div />]]></Html>        <Css><![CDATA[.right_to_left #this {  direction:rtl;  text-align:right;}]]></Css>        <Javascript><![CDATA[function(textObj) {     textObj.$el.html(textObj.text);    textObj.$el.find('input').remove();    textObj.$el.find('a').attr('target','_blank');    aa_lmc_zoom_paragraph(textObj.el,textObj.context,false);}]]></Javascript>      </Style>      <FieldAspect t="field_aspect.Hidden">        <OnCondition t="field_aspect.CheckConditionForEveryItem" DataForCondition="Item data" WhenHidden="Do not render">          <ShowCondition t="yesno.And">            <Item t="yesno.NotEmpty">              <Value t="text.FirstSucceeding">                <Item value="%html%"/>                <Item value="%@html%"/>              </Value>            </Item>            <Item t="yesno.IsInList" Item="%@type%">              <List t="data.List">                <Item value="rich text"/>                <Item value="html"/>              </List>            </Item>          </ShowCondition>        </OnCondition>      </FieldAspect>      <Text t="data.Pipeline">        <Item t="text.FirstSucceeding">          <Item value="%html%"/>          <Item value="%@html%"/>        </Item>        <Item t="lmc.ReplaceVariableFields"/>      </Text>      <FieldAspect t="field_aspect.CssOnCondition" ConditionInput="field data">        <Css><![CDATA[#this { padding: 10px; }]]></Css>        <OnCondition t="yesno.Expression">          <Expression t="yesno.And">            <Item t="yesno.Contains" IgnoreCase="true">              <Text value="table"/>            </Item>          </Expression>        </OnCondition>      </FieldAspect>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="paragraph_html"/>    </Field>    <Field t="control.Layout" ID="Grid" Title="Grid">      <Layout t="ui.CustomStyle" base="layout.Default">        <Html><![CDATA[<div><div class="field"/></div>]]></Html>        <Css><![CDATA[#this {}]]></Css>        <Javascript><![CDATA[function(layout) {  aa_layout(layout);  var hmargin = '%@hmargin%' || '0';  aa_lmc_zoom_paragraph(layout.el,layout.context,true,parseInt(hmargin));}]]></Javascript>      </Layout>      <FieldAspect t="field_feature.Hidden" ShowCondition="%@type% == 'grid'" DataForCondition="Item data" WhenHidden="Do not render"/>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="paragraph_grid"/>      <Field t="field.ItemList" ID="_ItemList" Title="ItemList">        <View t="ui.CustomStyle" base="itemlist_style.JBartTableNoHeader">          <Html><![CDATA[<div><table>  <tbody>  <tr class="aa_item">    <td class="aa_cell" />  </tr>  </tbody></table></div>]]></Html>          <Css><![CDATA[]]></Css>          <Javascript><![CDATA[function(itemlist) {    aa_itemlist_as_table(itemlist, { showHeaders: true } );}]]></Javascript>        </View>        <Field t="field.DynamicFields" ID="_DynamicFields" Title="DynamicFields">          <FieldItems t="data.Pipeline">            <Item t="data.CreateNumbers" From="1" To="%@cols%"/>          </FieldItems>          <Field t="control.Label" ID="_Label1" Title="Label">            <Style t="uitext.PlainText"/>            <Text t="lmc.GridValue" Value="%@col{$_FieldItem}%" />          </Field>        </Field>        <FieldAspect t="field_aspect.ItemListContainer">          <Items t="itemlist.Items" Items="%row%"/>        </FieldAspect>      </Field>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="highlight_first_row" ConditionForClass="%@highlightFirstRow% == 'true'"/>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ConditionForClass="%@highlightFirstColumn% == 'true'" ClassName="highlight_first_column"/>    </Field>    <Field t="field.Image" ID="_Image2" Title="Image">      <Image t="img.Image" Url="%@image%" OriginalWidth="%@imageWidth%" OriginalHeight="%@imageHeight%">        <FrameWidth t="img.ContainerWidth">          <Margin t="data.FirstSucceeding">            <Item value="0" Condition="%$InDesktopMasterArea%"/>            <Item value="%@imageWidthMargin%">              <Condition t="xml.HasAttribute" Attribute="imageWidthMargin"/>            </Item>            <Item t="text.FirstSucceeding" Condition="%$InMainMenu%">              <Item value="%$Room/style/general/@marginForImageInMainMenu%"/>              <Item value="0"/>            </Item>            <Item t="text.FirstSucceeding">              <Item value="%$Room/style/general/@marginForImageInItem%"/>              <Item value="10"/>            </Item>          </Margin>        </FrameWidth>        <AdjustSize t="img.Center" RightToLeft=""/>      </Image>      <Style t="ui.CustomStyle" base="image.PlainImage" InMainMenu="%$InMainMenu%" RTL="%$Room/style/general/@rtl%">        <Html><![CDATA[<div/>]]></Html>        <Css><![CDATA[#this {}]]></Css>        <Javascript><![CDATA[function(image) {  aa_lmcSetParagraphImage(image);}]]></Javascript>      </Style>      <FieldAspect t="field_aspect.Hidden">        <OnCondition t="field_aspect.CheckConditionForEveryItem">          <ShowCondition t="yesno.NotEmpty" Value="%@image%"/>        </OnCondition>      </FieldAspect>    </Field>    <Field t="youtube.YoutubeVideo" ID="_YoutubeVideo1" Title="YoutubeVideo" YoutubeKey="%@video%">      <Style t="ui.CustomStyle" base="youtube_style.ResponsiveVideo">        <Html><![CDATA[<div />]]></Html>        <Css><![CDATA[#this {  position: relative;  padding-bottom: 56.25%;  padding-top: 30px;  height: 0;  overflow: hidden; }iframe {  position: absolute;  top: 0;  left: 0;  width: 100%;  height: 100%;}  ]]></Css>        <Javascript><![CDATA[function render(youtubeVideo) {	  aa_lmc_render_youtube_video(youtubeVideo);	}]]></Javascript>      </Style>      <FieldAspect t="field_aspect.Hidden">        <OnCondition t="field_aspect.CheckConditionForEveryItem" WhenHidden="Do not render">          <ShowCondition t="yesno.NotEmpty" Value="%@video%"/>        </OnCondition>      </FieldAspect>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="paragraph_youtube"/>    </Field>    <FieldAspect t="lmc.CssFromSettings" CssID="paragraph"/>    <Field t="control.Layout" ID="_Vertical_Layout" Title="Property Sheet">      <Layout t="layout.Vertical" Spacing="0"/>      <Field t="control.Page" ID="_Link_to_page5" Title="Link to page">        <Page t="lmc.LMCRenderPropertySheet"/>      </Field>      <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Do not render">        <ShowCondition t="yesno.And">          <Item value="%@type% == 'property sheet'"/>          <Item t="yesno.Not">            <Of t="data.IsInCommaSeparatedList" List="%@aspects%" Item="show only in price quote"/>          </Item>        </ShowCondition>      </FieldAspect>    </Field>    <Field t="control.Layout" ID="_Vertical_Layout1" Title="price quote">      <Layout t="layout.Vertical" Spacing="0"/>      <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%@type% == 'price quote'"/>      <Field t="control.Page" ID="_Link_to_page5" Title="Link to page">        <Page t="lmc.LMCRenderPriceQuote"/>      </Field>    </Field>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="welcome_text" ConditionForClass="%@welcome% == 'true'"/>    <Field t="control.Button" ID="_Button" Title="Print" HideTitle="true" ButtonText="%@title%">      <Style t="ui.CustomStyle" base="btn.BlueButton">        <Html><![CDATA[<div>  <button class="print_button" /></div>]]></Html>        <Css><![CDATA[.right_to_left #this {  direction:rtl;  text-align:right;  display1:none;}.fld_VisitorDesktop #this {  display:block;}]]></Css>        <Javascript><![CDATA[function render(button) {  aa_button(button,{    textElement: button.$el.find('button'),    buttonElement: button.$el.find('button')   });}]]></Javascript>      </Style>      <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%@type% == 'print button'"/>      <Action t="lmc.LMCPrintCurrentItem" DocumentTitle="%@documentTitle%" HTmlTemplate="%htmlTemplate%">        <Css t="text.FirstSucceeding">          <Item value="%$Room/style/css[#print]%"/>          <Item value="%$Styles/[1]/styleProps/css[#print]%"/>        </Css>      </Action>    </Field>    <Field t="control.Button" ID="_Button1" Title="Next item" HideTitle="true">      <Style t="ui.CustomStyle" base="btn.BlueButton">        <Html><![CDATA[<div />]]></Html>        <Css><![CDATA[.right_to_left #this {  direction:rtl;  text-align:right;  display1:none;}.fld_VisitorDesktop #this {  display:block;}]]></Css>        <Javascript><![CDATA[function render(button) {  aa_button(button,{ });}]]></Javascript>      </Style>      <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%@type% == 'next item button'"/>      <Action t="data.JavaScript">        <Var name="NextItemID" t="data.Pipeline">          <Item t="xml.NextSibling" Data="%..%">            <Filter t="yesno.And">              <Item t="xml.IsOfTag" Tag="item"/>              <Item value="%@type% != 'heading'"/>            </Filter>          </Item>          <Item value="%@id%"/>        </Var>        <Code><![CDATA[function(data,html_elem,context) {    window.jbLMCGotoItem('%$NextItemID%');}]]></Code>      </Action>      <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="next_item_button"/>      <ButtonText t="data.Pipeline">        <Var name="NextItemName" t="data.Pipeline">          <Item value="%..%"/>          <Item t="xml.NextSibling">            <Filter t="yesno.And">              <Item t="xml.IsOfTag" Tag="item"/>              <Item value="%@type% != 'heading'"/>            </Filter>          </Item>          <Item value="%@name%"/>        </Var>        <Item t="text.FirstSucceeding">          <Item t="xtml.RunXtml" Input="%$NextItemName%" Xtml="%%" Field="title">&#xa;          </Item>          <Item t="xtml.RunXtml" Input="%$NextItemName%" Xtml="%$Room/settings%" Field="nextItemTitle"/>          <Item value="%$NextItemName%"/>        </Item>      </ButtonText>    </Field>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="hide_in_print" ConditionForClass="%@hideInPrint% == 'true'"/>    <FieldAspect t="field_feature.CssClass" AddClassTo="content">      <ClassName t="data.Pipeline">        <Var name="Type" t="text.Replace" Text="%@type%" Find=" " ReplaceWith="_"/>        <Item value="paragraph_%$Type%"/>      </ClassName>    </FieldAspect>    <Field t="control.Layout" ID="_Vertical_Layout" Title="Call me button">      <Layout t="layout.Vertical" Spacing="0"/>      <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%@type% == 'call me button'"/>      <Field t="control.TabControl" ID="callme_states" Title="Tab Control">        <Style t="tabs.HiddenTabs"/>        <Field t="control.Layout" ID="_Group" Title="Call me">          <Field t="control.Label" ID="_Label" Title="Call me pre text" Text="%@preText%">            <Style t="uitext.PlainText"/>            <FieldAspect t="field_feature.Hidden" DataForCondition="Item data" WhenHidden="Do not render" ShowCondition="%@preText% != ''"/>            <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="callme_pretext"/>          </Field>          <Layout t="layout.Default"/>          <Field t="control.Button" ID="_Button" Title="Call me" HideTitle="true" ButtonText="%@title%">            <Style t="ui.CustomCss" base="btn.JBart">              <Style t="btn.JBart" MinWidth="60px"/>              <Css><![CDATA[#this {}]]></Css>            </Style>            <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="callme_button callme"/>            <Action t="action.RunActions">              <Action t="lmc.CallMeButtonClicked"/>              <Action t="operation.ChangeTab" TabControl="callme_states" MoveTo="next tab"/>            </Action>          </Field>        </Field>        <Field t="control.Layout" ID="_Group" Title="Success">          <Field t="control.Label" ID="success" Title="Success" Text="%@successText%">            <Style t="uitext.PlainText"/>            <FieldAspect t="field_aspect.Title"/>            <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="callme_success callme"/>          </Field>          <Layout t="layout.Default"/>        </Field>      </Field>    </Field>  </xtml></Component><Component id="LMCRenderPriceQuote" type="jbart.MyWidgetPage">  <Param name="RoomXml" Default="%$Room%"/>  <xtml t="control.Layout" ID="_Vertical_Layout" Title="Price quote">    <Field t="lmc.LMCRenderPropertySheet">      <Properties t="data.IfThenElse" If="%@manualSummary%" Then="%property%">        <Else t="data.Pipeline">          <Item t="data.List">            <Item value="%$RoomXml///paragraph[@type='property sheet']%"/>            <Item value1="%$RoomXml/items/paragraph[@type='property sheet']%"/>          </Item>          <Item t="yesno.PassesFilter">            <Filter t="data.IsInCommaSeparatedList" Item="show only in price quote" List="%@aspects%"/>          </Item>          <Item value="%property%"/>        </Else>      </Properties>    </Field>    <Layout t="layout.Default"/>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="price_quote"/>  </xtml></Component><Component id="LMCRenderPropertySheet" type="jbart.MyWidgetPage">  <Param name="Properties" Default="%property%"/>  <xtml t="field.ItemList" Title="Property Sheet" ID="items_table">    <View t="ui.CustomStyle" base="itemlist_style.JBartTable">      <Html><![CDATA[<div><table>  <tbody>  <tr class="aa_item">    <td class="aa_cell" />  </tr>  </tbody></table></div>]]></Html>      <Css><![CDATA[]]></Css>      <Javascript><![CDATA[function(itemlist) {    aa_itemlist_as_table(itemlist, { showHeaders: true, titleClass: 'aa_table_title' } );}]]></Javascript>    </View>    <FieldAspect t="field_aspect.ItemListContainer">      <Items t="itemlist.Items" Items="%$Properties%"/>    </FieldAspect>    <Field t="control.Label" ID="_Label" Title="Name" Text="%@name%">      <Style t="ui.CustomStyle" base="uitext.PlainText">        <Html><![CDATA[<div />]]></Html>        <Css><![CDATA[#this { font-weight: bold; }]]></Css>        <Javascript><![CDATA[function(textObj) {   textObj.$el.html(textObj.text);}]]></Javascript>      </Style>      <FieldAspect t="field_feature.HandleEvent">        <Event t="field_feature.ControlAttached"/>        <Action t="operation.RunActionInJavascript">          <Code><![CDATA[function run(data,context) {  var elem = context.vars.ControlElement[0];  var item = context.vars._Item[0];  if (item.getAttribute("section") == "true") {    $(elem).parent().attr("colspan","2");    $(elem).parent().next().remove();    $(elem).parent().addClass("paragraph_property_section");  }}]]></Code>        </Action>      </FieldAspect>    </Field>    <Field t="fld.Field" ID="value" Title="Value">      <FieldType t="fld_type.Text">        <Style t="uitext.PlainText"/>      </FieldType>      <FieldAspect t="field_aspect.RunAction">        <AfterControlCreated t="operation.RunActionInJavascript">          <Code><![CDATA[function run(data,context) { var elem = context.vars.ControlElement[0];}]]></Code>        </AfterControlCreated>      </FieldAspect>      <FieldData t="lmc.GridValue" Value="%@value%"/>    </Field>    <FieldAspect t="field_feature.CssClass" AddClassTo="content" ClassName="paragraph_property_sheet"/>    <FieldAspect t="field_feature.Css">      <Css><![CDATA[.right_to_left #this { direction: rtl; }]]></Css>    </FieldAspect>  </xtml></Component><Component id="GridValue" type="data.Data">  <Param name="Value"/>  <xtml t="data.IfThenElse" Else="%$Value%">    <If t="yesno.Contains" AllText="%$Value%">      <Text value="=Sum()"/>    </If>    <Then t="text.Replace" Text="%$Value%" Find="=Sum\(\)">      <ReplaceWith t="data.Pipeline">        <Item t="data.Pipeline">          <Var name="ColumnName" t="xml.AttributeName" Data="%$Value%"/>          <Item value="%$ItemListCntr/Items/@{$ColumnName}%"/>          <Item t="text.Replace" Find="[^0-9\.]*"/>          <Aggregator t="data.Sum"/>        </Item>        <Item t="text.NumberFormat" Cents="false"/>      </ReplaceWith>    </Then>  </xtml></Component></xtml>