<file _type="jbart_project" name="Order Tickets Done" id="Order_Tickets_Done" vid="1">  <xtml ns="sample">    <Component id="Widget" type="jbart.Widget">      <xtml t="jbart.Widget">        <MainPage t="sample.main"/>        <DataResource t="jbart_resource.Data" ResourceID="Data" ValueType="xml" AutoSaveSampleData="">          <Value t="xml.Xml">            <xml/>          </Value>        </DataResource>        <ApplicationFeature t="appfeature.Css">          <Css><![CDATA[#this { font: 12px Arial; }      #this *:focus { outline:none; }      p { margin: 0}       ]]></Css>        </ApplicationFeature>        <ApplicationFeature t="appfeature.Responsive"/>        <DataResource t="jbart_resource.Data" ResourceID="Merchandise" ValueType="xml" AutoSaveSampleData="">          <Value t="xml.Xml">            <Merchandise>              <item name="Hat" price="10"/>              <item name="T-Shirt" price="15"/>              <item name="Cup" price="5"/>            </Merchandise>          </Value>        </DataResource>      </xtml>    </Component>    <Component id="main" type="jbart.MyWidgetPage">      <xtml t="control.Layout" ID="main" Title="Order Ticket">        <FieldAspect t="field_aspect.FieldData" FieldData="%$Data%"/>        <Layout t="layout.Vertical" Spacing="30px"/>        <Field t="control.PropertySheet" ID="_Property_Sheet" Title="Property Sheet">          <Field t="fld.Field" FieldData="%!@name%" ID="name" Title="Name">            <FieldType t="fld_type.EditableText">              <Style t="editable_text.JBartTextbox" Width="300px"/>            </FieldType>          </Field>          <Style t="properties.TitlesAboveFields" HorizontalSpacing="58px" VerticalSpacing="8px"/>        </Field>        <Field t="control.PropertySheet" ID="_Property_Sheet1" Title="Property Sheet">          <Style t="properties.TitlesAboveFields" HorizontalSpacing="50px" VerticalSpacing="8px"/>          <Field t="fld.Field" FieldData="%!@movie%" ID="movie" Title="Movie">            <FieldType t="fld_type.Picklist" AllowEmptyValue="" AllowValueNotInOptions="">              <Options t="editable_picklist.OptionsByCommas" Options="Frozen,The Aviator,The purple rose of cairo"/>              <Style t="editable_picklist.JBartPicklist"/>            </FieldType>          </Field>          <Field t="fld.Field" FieldData="%!@no_of_tickets%" ID="no_of_tickets" Title="No Of Tickets">            <FieldType t="fld_type.Picklist" AllowEmptyValue="" AllowValueNotInOptions="">              <Options t="editable_picklist.OptionsByCommas" Options="1,2,3"/>              <Style t="editable_picklist.RadioButtons" AddRadioButtonForEmptyValue=""/>            </FieldType>          </Field>        </Field>        <Field t="field.ItemListContainer" ID="items-cntr" Title="items" HideTitle="">          <Items t="itemlist.XmlItems" ParentXml="%%" Tag="item"/>          <Field t="control.Layout" ID="items-cntr_Toolbar" Title="Toolbar">            <Layout t="layout.Horizontal" Spacing="5px"/>            <FieldAspect t="itemlist_aspect.RefreshOnSelectionChange"/>            <FieldAspect t="field_feature.Layout">              <Css><![CDATA[#this { margin-bottom: 5px; }]]></Css>            </FieldAspect>            <Field t="control.Button" ID="items-cntr_Add" Title="Add item">              <Style t="btn.HyperlinkWithImage"/>              <Image t="img.Image" Url="_jbartImages_/add1616.gif" OriginalWidth="16" OriginalHeight="16"/>              <Action t="itemlist_action.AddItemToItemList">                <Item t="xml.ByTag" Tag="item"/>              </Action>            </Field>            <Field t="control.Button" ID="items-cntr_Delete" Title="Delete item">              <Style t="btn.HyperlinkWithImage"/>              <Image t="img.Image" Url="_jbartImages_/delete1616.gif" OriginalWidth="16" OriginalHeight="16"/>              <Action t="itemlist_action.DeleteSelectedItem"/>            </Field>          </Field>          <Field t="field.ItemList" Title="items" ID="items-table">            <View t="itemlist_style.JBartTable" MinWidth="300px" Padding="5px"/>            <FieldAspect t="itemlist_aspect.ItemSelection"/>            <FieldAspect t="itemlist_aspect.TableColumnDragAndDrop"/>            <FieldAspect t="itemlist_aspect.DragAndDropItems"/>            <FieldAspect t="itemlist_aspect.ShowTextWhenNoItems"/>            <FieldAspect t="itemlist_aspect.UpdateOnAddOrDeleteItem"/>            <Field t="fld.Field" FieldData="%!@name%" ID="name" Title="Name">              <FieldType t="fld_type.Picklist" AllowEmptyValue="" AllowValueNotInOptions="">                <Options t="editable_picklist.DynamicOptions" Options="%$Merchandise/item/@name%"/>                <Style t="editable_picklist.JBartPicklist"/>              </FieldType>            </Field>            <Field t="fld.Field" FieldData="%!@quantity%" ID="quantity" Title="Quantity">              <FieldType t="fld_type.EditableText">                <Style t="editable_text.JBartTextbox" Width="125px"/>              </FieldType>            </Field>          </Field>          <Layout t="layout.Default"/>        </Field>      </xtml>    </Component>  </xtml></file>